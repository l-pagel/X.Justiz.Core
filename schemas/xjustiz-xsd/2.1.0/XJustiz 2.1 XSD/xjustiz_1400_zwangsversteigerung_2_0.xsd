                    <xs:element name="datum" type="tns:Type.GDS.Datumsangabe" />
                    <xs:element name="uhrzeit" type="tns:Type.GDS.Zeitangabe" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="veroeffentlichung">
