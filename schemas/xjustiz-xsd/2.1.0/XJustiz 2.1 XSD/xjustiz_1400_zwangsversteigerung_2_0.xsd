            <xs:documentation />
          </xs:annotation>
        </xs:element>
        <xs:element name="grunddaten">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="verfahrensdaten">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="instanzdaten">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="sachgebiet" minOccurs="0" type="tns:Code.GDS.Sachgebiet" />
                          <xs:element name="instanzbehoerde">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="gericht" type="tns:Code.GDS.Gerichte" />
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="aktenzeichen" type="tns:Type.GDS.Aktenzeichen" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="termin">
                      <xs:annotation>
                        <xs:documentation>Termin der Versteigerung</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="terminsdaten">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="terminsort">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="gerichtsort">
                                        <xs:complexType>
                                          <xs:sequence>
                                            <xs:element name="gebaeude" minOccurs="0" type="xoev-lc:String.Latin" />
                                            <xs:element name="anschrift" minOccurs="0" type="tns:Type.GDS.Anschrift" />
                                            <xs:element name="stockwerk" minOccurs="0" type="xoev-lc:String.Latin" />
                                            <xs:element name="raum" type="xoev-lc:String.Latin" />
                                          </xs:sequence>
                                        </xs:complexType>
                                      </xs:element>
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                                <xs:element name="terminszeit">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="terminsdatum" type="xs:date" />
                                      <xs:element name="terminsuhrzeit" type="xs:time" />
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="sendungsdaten" minOccurs="0">
