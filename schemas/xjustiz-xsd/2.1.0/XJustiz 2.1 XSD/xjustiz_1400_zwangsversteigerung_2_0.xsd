  <xs:import schemaLocation="xdomea_typen_1_0.xsd" namespace="http://www.xdomea.de" />
  <xs:import schemaLocation="latinchars.xsd" namespace="http://xoev.de/latinchars/1_1/datatypes" />
  <xs:import schemaLocation="xoev-code.xsd" namespace="http://xoev.de/schemata/code/1_0" />
  <xs:complexType name="Type.ZVG.Zahlung">
    <xs:sequence>
      <xs:element name="datum" type="xs:date" />
      <xs:element name="betrag" type="tns:Type.GDS.Geldbetrag" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="nachricht.zvg.zwangsversteigerungstermin.1400001">
