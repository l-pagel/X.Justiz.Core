                    <xs:element name="glaeubiger" minOccurs="0" type="xoev-lc:String.Latin" />
                    <xs:element name="verkehrswert" type="xoev-lc:String.Latin" />
                    <xs:element name="bemerkung" minOccurs="0" type="xoev-lc:String.Latin" />
                    <xs:element name="aenderung" minOccurs="0" type="xs:integer" />
                    <xs:element name="hinweis" minOccurs="0" type="xoev-lc:String.Latin" />
                    <xs:element name="geoServer" minOccurs="0" type="xoev-lc:String.Latin" />
                    <xs:element name="googleMaps" type="xoev-lc:String.Latin" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>

