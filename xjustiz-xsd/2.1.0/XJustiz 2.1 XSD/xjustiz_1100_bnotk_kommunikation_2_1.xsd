    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="tns:Type.BNOTK.Kopf_Und_Grunddaten.Input">
          <xs:sequence>
            <xs:element name="fachdaten">
              <xs:complexType>
                <xs:sequence>
                  <xs:choice>
                    <xs:element name="id_Suche">
                      <xs:annotation>
                        <xs:documentation>Die Id-Suche sucht immer nach eindeutigen Eigenschaften und liefert keine oder maximal eine Amtstätigkeit.</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:choice>
                          <xs:element name="amtstaetigkeit" type="xoev-lc:String.Latin">
                            <xs:annotation>
