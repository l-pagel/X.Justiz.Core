                              </xs:extension>
                            </xs:complexContent>
                          </xs:complexType>
                        </xs:element>
                        <xs:element name="erstes_Entscheidungsdatum" minOccurs="0" type="xs:date" />
                        <xs:element name="rechtskraft" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Rechtskraft" />
