                  <xs:element name="beteiligten_IDs" minOccurs="0" type="tns:Type.STRAF.Beteiligten_IDs" />
                  <xs:element name="tatvorwurf" minOccurs="0" maxOccurs="unbounded" type="tns:Type.STRAF.Tatvorwurf">
