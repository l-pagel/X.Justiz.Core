   <xs:element name="nachricht.straf.asservate.0500017">
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="tns:Type.GDS.Basisnachricht">
               <xs:sequence>
                  <xs:element name="schriftgutobjekte" minOccurs="0">
                     <xs:complexType>
                        <xs:complexContent>
                           <xs:restriction base="tns:Type.GDS.Schriftgutobjekte">
                              <xs:sequence>
                                 <xs:element name="dokument"
                                             minOccurs="0"
                                             maxOccurs="unbounded"
                                             type="tns:Type.GDS.Dokument"/>
                              </xs:sequence>
                           </xs:restriction>
                        </xs:complexContent>
                     </xs:complexType>
                  </xs:element>
                  <xs:element name="fachdaten">
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element name="instanzdaten" minOccurs="0" maxOccurs="unbounded">
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element name="ref.instanznummer" type="xoev-lc:String.Latin"/>
                                    <xs:element name="aktenzeichen"
                                                minOccurs="0"
                                                maxOccurs="unbounded"
                                                type="tns:Type.GDS.Aktenzeichen"/>
                                 </xs:sequence>
                              </xs:complexType>
                           </xs:element>
                           <xs:element name="anbietung" minOccurs="0" maxOccurs="unbounded">
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element name="nummer" minOccurs="0" type="xoev-lc:String.Latin">
                                       <xs:annotation>
                                          <xs:documentation>Da von anderen Elementen auf schon erfasste Asservate verwiesen werden soll, ist ein eindeutiges Nummern-Feld für das Element 'Asservat' notwendig.</xs:documentation>
                                       </xs:annotation>
                                    </xs:element>
                                    <xs:element name="asservatenID" type="xoev-lc:String.Latin"/>
                                    <xs:element name="gegenstandsart" type="xoev-lc:String.Latin"/>
                                    <xs:element name="aufbewahrungsbehoerde"
                                                minOccurs="0"
                                                type="tns:Type.GDS.Behoerde"/>
                                    <xs:element name="gefahrgut" minOccurs="0" type="xs:boolean"/>
                                    <xs:element name="lagerhinweis" minOccurs="0" type="xoev-lc:String.Latin"/>
                                    <xs:element name="bezeichnung" type="xoev-lc:String.Latin"/>
                                    <xs:element name="menge" type="xoev-lc:String.Latin">
                                       <xs:annotation>
                                          <xs:documentation>Hier kann die Menge als Freitext erfasst werden.</xs:documentation>
                                       </xs:annotation>
                                    </xs:element>
                                    <xs:element name="einheit" minOccurs="0" type="xoev-lc:String.Latin"/>
                                    <xs:element name="beteiligter"
                                                minOccurs="0"
                                                maxOccurs="unbounded"
                                                type="tns:Type.GDS.Ref.Rollennummer">
                                       <xs:annotation>
                                          <xs:documentation>Für jede Beteiligung gibt es genau einen Beteiligten, der hier durch einen Verweis auf die Rollennummer des Beteiligten im Grunddatensatz referenziert wird.</xs:documentation>
                                       </xs:annotation>
                                    </xs:element>
                                    <xs:element name="laufendeNummer" minOccurs="0" type="xoev-lc:String.Latin">
                                       <xs:annotation>
                                          <xs:documentation>Die laufende Nummer in der Asservatengruppe. z.B. Nr.4</xs:documentation>
                                       </xs:annotation>
                                    </xs:element>
                                 </xs:sequence>
                              </xs:complexType>
                           </xs:element>
                           <xs:element name="uebermittlung" minOccurs="0" maxOccurs="unbounded">
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element name="nummer" minOccurs="0" type="xoev-lc:String.Latin">
                                       <xs:annotation>
                                          <xs:documentation>Da von anderen Elementen auf schon erfasste Asservate verwiesen werden soll, ist ein eindeutiges Nummern-Feld für das Element 'Asservat' notwendig.</xs:documentation>
                                       </xs:annotation>
                                    </xs:element>
                                    <xs:element name="asservatenID" type="xoev-lc:String.Latin"/>
                                    <xs:element name="gegenstandsart" type="xoev-lc:String.Latin"/>
                                    <xs:element name="aufbewahrungsbehoerde"
                                                minOccurs="0"
                                                type="tns:Type.GDS.Behoerde"/>
                                    <xs:element name="gefahrgut" minOccurs="0" type="xs:boolean"/>
                                    <xs:element name="lagerhinweis" minOccurs="0" type="xoev-lc:String.Latin"/>
                                    <xs:element name="bezeichnung" type="xoev-lc:String.Latin"/>
                                    <xs:element name="menge" type="xoev-lc:String.Latin">
                                       <xs:annotation>
                                          <xs:documentation>Hier kann die Menge als Freitext erfasst werden.</xs:documentation>
                                       </xs:annotation>
                                    </xs:element>
                                    <xs:element name="einheit" minOccurs="0" type="xoev-lc:String.Latin"/>
                                    <xs:element name="beteiligter"
                                                minOccurs="0"
                                                maxOccurs="unbounded"
                                                type="tns:Type.GDS.Ref.Rollennummer">
                                       <xs:annotation>
                                          <xs:documentation>Für jede Beteiligung gibt es genau einen Beteiligten, der hier durch einen Verweis auf die Rollennummer des Beteiligten im Grunddatensatz referenziert wird.</xs:documentation>
                                       </xs:annotation>
                                    </xs:element>
                                    <xs:element name="laufendeNummer" minOccurs="0" type="xoev-lc:String.Latin">
                                       <xs:annotation>
                                          <xs:documentation>Die laufende Nummer in der Asservatengruppe. z.B. Nr.4</xs:documentation>
                                       </xs:annotation>
                                    </xs:element>
                                 </xs:sequence>
                              </xs:complexType>
                           </xs:element>
                           <xs:element name="rueckmeldung" minOccurs="0" maxOccurs="unbounded">
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element name="asservatenID" type="xoev-lc:String.Latin"/>
                                    <xs:element name="status" type="tns:Code.STRAF.Asservat.Rueckmeldung.Typ3"/>
                                 </xs:sequence>
                              </xs:complexType>
                           </xs:element>
                           <xs:element name="uebernahme" minOccurs="0" maxOccurs="unbounded">
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element name="asservatenID" type="xoev-lc:String.Latin"/>
                                    <xs:element name="status">
                                       <xs:complexType>
                                          <xs:sequence>
                                             <xs:element name="uebernahme" type="xs:boolean"/>
                                             <xs:element name="veraendert" minOccurs="0" type="xs:boolean"/>
                                             <xs:element name="grund" minOccurs="0" type="xoev-lc:String.Latin">
                                                <xs:annotation>
                                                   <xs:documentation>Grund für die Nichtübernahme des Asservates z.B. fehlende Anlieferung, falsche Menge etc.</xs:documentation>
                                                </xs:annotation>
                                             </xs:element>
                                          </xs:sequence>
                                       </xs:complexType>
                                    </xs:element>
                                 </xs:sequence>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                        <xs:attribute name="fachdatenStrafVersion"
                                      type="xoev-lc:String.Latin"
                                      use="required"
                                      fixed="1.0"/>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="nachricht.straf.empfangsbestaetigung.0500018">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="nachrichtenkopf" type="tns:Type.GDS.Nachrichtenkopf"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="nachricht.straf.fehlermitteilung.0500019">
      <xs:complexType>
