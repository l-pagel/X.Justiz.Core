          </xs:complexType>
        </xs:element>
        <xs:element name="fachdaten">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="nachrichten_ID" minOccurs="0" type="xoev-lc:String.Latin" />
              <xs:element name="antwort_ID" minOccurs="0" type="xoev-lc:String.Latin" />
              <xs:element name="fehlercode" type="xs:integer" />
              <xs:element name="fehlertext" minOccurs="0" type="xoev-lc:String.Latin" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
