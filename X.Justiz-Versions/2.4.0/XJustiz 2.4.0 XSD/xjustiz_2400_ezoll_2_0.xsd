      <xs:sequence>
        <xs:element name="nachrichtenkopf">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="aktenzeichen.Empfaenger" type="xoev-lc:String.Latin" />
              <xs:element name="erstellungszeitpunkt" type="xs:dateTime" />
              <xs:element name="absender" type="xoev-lc:String.Latin" />
              <xs:element name="empfaenger" type="xoev-lc:String.Latin" />
              <xs:element name="ereignis" type="tns:Code.EZOLL.Ereignis" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="grunddaten">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="verfahrensdaten" minOccurs="0">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="herstellerinformation" type="tns:Type.GDS.Herstellerinformation" />
                  </xs:sequence>
                </xs:complexType>
