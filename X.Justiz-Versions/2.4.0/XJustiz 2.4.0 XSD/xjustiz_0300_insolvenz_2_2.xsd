<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://www.xjustiz.de" xmlns:xdo="http://www.xdomea.de" xmlns:xdomea="http://www.xdomea.de/V2.2.0" xmlns:xoev-code="http://xoev.de/schemata/code/1_0" xmlns:xoev-lc="http://xoev.de/latinchars/1_1/datatypes" targetNamespace="http://www.xjustiz.de" version="2.2.0" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xs:include schemaLocation="xjustiz_0000_grunddatensatz_2_4.xsd" />
  <xs:include schemaLocation="xjustiz_0310_cl_insolvenz_grund_2_0.xsd" />
  <xs:include schemaLocation="xjustiz_0320_cl_insolvenz_2_1.xsd" />
  <xs:include schemaLocation="xjustiz_0330_cl_nachrichten_2_2.xsd" />
  <xs:import schemaLocation="xdomea_typen_1_0.xsd" namespace="http://www.xdomea.de" />
  <xs:import schemaLocation="xdomea-Baukasten.xsd" namespace="http://www.xdomea.de/V2.2.0" />
  <xs:import schemaLocation="latinchars.xsd" namespace="http://xoev.de/latinchars/1_1/datatypes" />
  <xs:import schemaLocation="xoev-code.xsd" namespace="http://xoev.de/schemata/code/1_0" />
  <xs:complexType name="Type.INSO.Aktenzeichen">
    <xs:complexContent>
      <xs:restriction base="tns:Type.GDS.Aktenzeichen">
        <xs:sequence>
          <xs:element name="abteilung" minOccurs="0" type="xoev-lc:String.Latin">
            <xs:annotation>
              <xs:documentation>Abteilungs-/Dezernats-/Geschäftsstellenbezeichnung</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="register" type="tns:Code.GDS.Registerzeichen" />
          <xs:element name="laufendeNummer" type="xoev-lc:String.Latin" />
          <xs:element name="jahr" type="xoev-lc:String.Latin" />
          <xs:element name="zusatz" minOccurs="0" type="xoev-lc:String.Latin">
            <xs:annotation>
              <xs:documentation>Ein optionaler Zustz für das Aktenzeichen</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="Type.INSO.Berichtsjahr_Listen">
    <xs:sequence>
      <xs:element name="jahr" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Hier ist das betreffende Berichtsjahr (4-stellig), für das die Verfahren gemeldet wurden, anzugeben.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MeldArtRB" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="verfahren" maxOccurs="unbounded">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="urspruenglichesAz" type="tns:Type.GDS.Aktenzeichen" />
                  <xs:element name="verfahrensId" type="xoev-lc:String.Latin" />
                  <xs:element name="eroeffnungsdatum" type="xs:date" />
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="MeldArtVB" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="verfahren" maxOccurs="unbounded">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="urspruenglichesAz" type="tns:Type.GDS.Aktenzeichen" />
                  <xs:element name="verfahrensId" type="xoev-lc:String.Latin" />
                  <xs:element name="eroeffnungsdatum" type="xs:date" />
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="MeldArtX" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="verfahren" maxOccurs="unbounded">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="urspruenglichesAz" type="tns:Type.GDS.Aktenzeichen" />
                  <xs:element name="verfahrensId" type="xoev-lc:String.Latin" />
                  <xs:element name="eroeffnungsdatum" type="xs:date" />
                  <xs:element name="widerruf" type="xs:boolean">
                    <xs:annotation>
                      <xs:documentation>Ja bei Widerruf der Erteilung der RSB</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.INSO.Fachdaten">
    <xs:sequence>
      <xs:element name="sicherungsmassnahme" minOccurs="0">
        <xs:annotation>
          <xs:documentation>§ 21 InsO Sicherungsmassnahmen sind Anordnungen des Insolvenzgerichts zur Verhütung einer für die Gläubiger nachteiligen Veränderung der Vermögenslage des Schuldners bis zur Entscheidung über den Insolvenzantrag.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="massnahmeart" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>z.B. allgemeines Verfügungsverbot</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="auswahl_anordnung_aufhebung">
              <xs:complexType>
                <xs:choice>
                  <xs:element name="aufhebungsdatum" minOccurs="0" type="xs:date">
                    <xs:annotation>
                      <xs:documentation>Datumsfeld gem. DIN ISO 8601 (yyyy-mm-dd)</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="anordnung" minOccurs="0">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="anordnungsdatum" type="xs:date">
                          <xs:annotation>
                            <xs:documentation>Datumsfeld gem. DIN ISO 8601 (yyyy-mm-dd)</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="anordnungszeit" minOccurs="0" type="xs:time">
                          <xs:annotation>
                            <xs:documentation>Uhrzeit gem. DIN ISO 8601 (hh:mm)</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                </xs:choice>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="auswahl_datum">
        <xs:complexType>
          <xs:choice>
            <xs:element name="beendigungsdatum" minOccurs="0" type="xs:date">
              <xs:annotation>
                <xs:documentation>Das Ende des Verfahrens. Datumsfeld gem. DIN ISO 8601 (yyyy-mm-dd)</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="eroeffnung" minOccurs="0">
              <xs:annotation>
                <xs:documentation>§§ 11 ff. Ein Insolvenzverfahren wird nur auf Antrag eröffnet. Daten bezüglich der Eröffnung des Verfahrens -</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="eroeffnungsdatum" type="xs:date">
                    <xs:annotation>
                      <xs:documentation>Datum der Eröffung des Insolvenzverfahrens durch Beschluß. Datumsfeld gem. DIN ISO 8601 (yyyy-mm-dd)</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="eroeffnungszeit" minOccurs="0" type="xs:time">
                    <xs:annotation>
                      <xs:documentation>Uhrzeit gem. DIN ISO 8601 (hh:mm)</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="anmeldung">
                    <xs:annotation>
                      <xs:documentation>Die Anmeldefrist, in der die Gläubiger ihre Forderungen beim Verwalter anmelden können. Die Frist beläuft sich auf 2 Wochen bis höchstens 3 Monate ab Eröffnung des Verfahrens. Kann als Frist oder als Datumsangabe angegeben werden</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:choice>
                        <xs:element name="anmeldefrist" type="xoev-lc:String.Latin">
                          <xs:annotation>
                            <xs:documentation>Hier steht die textuelle Angabe der Frist</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="anmeldedatum" type="xs:date">
                          <xs:annotation>
                            <xs:documentation>Datumsfeld gem. DIN ISO 8601 (yyyy-mm-dd)</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                      </xs:choice>
                    </xs:complexType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:choice>
        </xs:complexType>
      </xs:element>
      <xs:element name="eigenverwaltung" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Die Verwaltungs- und Verfügungsbefugnis geht bei Eröffnung eines Insolvenzverfahrens nicht auf einen Insolvenzverwalter über, sondern sie verbleibt beim Schuldner. Der Schuldner steht unter Aufsicht des gerichtlich bestellten Sachwalters, §§ 270 - 285 InsO. Diese Anordnung erfolgt von Amts wegen und wird durch Anordnung auch wieder aufgehoben.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="auswahl_datum">
              <xs:complexType>
                <xs:choice>
                  <xs:element name="anordnungsdatum" minOccurs="0" type="xs:date">
                    <xs:annotation>
                      <xs:documentation>Das Datum der Anordnung der Eigenverwaltung. Datumsfeld gem. DIN ISO 8601 (yyyy-mm-dd)</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="aufhebungsdatum" minOccurs="0" type="xs:date">
                    <xs:annotation>
                      <xs:documentation>Das Datum der Aufhebung der Eigenverwaltung. Datumsfeld gem. DIN ISO 8601 (yyyy-mm-dd)</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:choice>
              </xs:complexType>
            </xs:element>
            <xs:element name="auswahl_datum_zustimmungsvorbehalt">
              <xs:complexType>
                <xs:choice>
                  <xs:element name="anordnungsdatumDesZustimmungsvorbehalts" minOccurs="0" type="xs:date">
                    <xs:annotation>
                      <xs:documentation>Das Datum der Anordnung des Zustimmungsvorbehalts. Datumsfeld gem. DIN ISO 8601 (yyyy-mm-dd) Die Anordnung des Zustimmungsvorbehaltes erfolgt durch das Gericht. Sie bedeutet, dass der Schuldner ohne Zustimmung des Insolvenzverwalters nicht mehr über Werte verfügen kann. Geschäfte des Schuldners bedürfen damit der Zustimmung des vorläufigen Insolvenzverwalters.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="aufhebungsdatumDesZustimmungsvorbehalts" minOccurs="0" type="xs:date">
                    <xs:annotation>
                      <xs:documentation>Das Datum der Aufhebung des Zustimmungsvorbehalts. Datumsfeld gem. DIN ISO 8601 (yyyy-mm-dd)</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:choice>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="auswahl_insolvenzplan" minOccurs="0">
        <xs:annotation>
          <xs:documentation>§§ 217-269 InsO Der Insolvenzplan kann von dem Schuldner oder dem Insolvenzverwalter vorgelegt werden. In dem Insolvenzplan kann eine von der Regelinsolvenz abweichende Verwertung des Vermögens, Verteilung u. Befriedigung der Gläubiger  und restliche Haftung des Schuldners vereinbart werden.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice>
            <xs:element name="datum.anordnungDerUeberwachung" minOccurs="0" type="xs:date">
              <xs:annotation>
                <xs:documentation>§ 260 InsO Überwachung der Planerfüllung. Im gestaltenden Teil des Insolvenzplans kann vorgesehen werden, daß die Erfüllung des Plans überwacht wird. Datum des Beschlusses, § 267 InsO. Datumsfeld gem. DIN ISO 8601 (yyyy-mm-dd)</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="datum.aufhebungDerUeberwachung" minOccurs="0" type="xs:date">
              <xs:annotation>
                <xs:documentation>Datum des Beschlusses, § 268 InsO. Datumsfeld gem. DIN ISO 8601 (yyyy-mm-dd)</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
        </xs:complexType>
      </xs:element>
      <xs:element name="auswahl_restschuldbefreiung" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Der redliche Schuldner kann von seinen restlichen Schulden befreit werden.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice>
            <xs:element name="versagungsdatum" minOccurs="0" type="xs:date">
              <xs:annotation>
                <xs:documentation>Datumsfeld gem. DIN ISO 8601 (yyyy-mm-dd)</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="ankuendigungsdatum" minOccurs="0" type="xs:date">
              <xs:annotation>
                <xs:documentation>Datumsfeld gem. DIN ISO 8601 (yyyy-mm-dd)</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="erteilungsdatum" minOccurs="0" type="xs:date">
              <xs:annotation>
                <xs:documentation>Datumsfeld gem. DIN ISO 8601 (yyyy-mm-dd)</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="widerrufsdatum" minOccurs="0" type="xs:date">
              <xs:annotation>
                <xs:documentation>Datumsfeld gem. DIN ISO 8601 (yyyy-mm-dd)</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
        </xs:complexType>
      </xs:element>
      <xs:element name="veroeffentlichung" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Veröffentlichungen nach § 9 InsO</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="ref.dokument" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Referenz auf ein Dokument im Grunddatensatz</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="auswahl_loeschung" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Löschfristen gem. § 9 Abs.2 S.3 InsO</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:choice>
                  <xs:element name="loeschdatum" minOccurs="0" type="xs:date">
                    <xs:annotation>
                      <xs:documentation>Datumsfeld gem. DIN ISO 8601 (yyyy-mm-dd)</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="loeschfrist" minOccurs="0" type="xoev-lc:String.Latin">
                    <xs:annotation>
                      <xs:documentation>string</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:choice>
              </xs:complexType>
            </xs:element>
            <xs:element name="gegenstandDerVeroeffentlichung" minOccurs="0" type="tns:Code.INSO.Veroeffentlichungsgegenstand">
              <xs:annotation>
                <xs:documentation>Es liegt keine Beschreibung vor</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="mitarbeiterkennziffer" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>An den Mitarbeiter des Insolvenzgerichts vergebene Kennziffer</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="unternehmen" type="xs:boolean">
              <xs:annotation>
                <xs:documentation>Als "Unternehmen" zu verstehen sind alle Schuldner, die im deutschen Handels-, Genossenschafts-, Partnerschafts- oder Vereinsregister eingetragen sind.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="abstimmungsgruppen" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>§§ 222, 243 InsO; Gruppen werden im Insolvenzplan festgelegt, sie stimmen gesondert über den Plan ab.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="identifier" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Es liegt keine Beschreibung vor</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="name.abstimmungsgruppe" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Name wird im Plan bestimmt.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="kennzeichen.gesamtglaeubiger">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="identifier" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Forderungen, die zu einer "Gesamtgläubigerschaft" gehören und auf mehrere Tabellenblättern getrennt nach den Gesamtgläubigern verteilt sind, erhalten dasselbe Kennzeichen. Am einfachsten fortl. Nummer je Gesamtgläubigerschaft beginnend mit 1. Dieses Element ist über das gesamte  Insolvenzverfahren eindeutig und gleichlautend zu halten Es bleibt dem jeweiligen Gerichtsprogramm überlassen, die Gesamtgläubigerschaften zu verwalten.
Maximal 10 Zeichen.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="beteiligte.inso" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Es liegt keine Beschreibung vor</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="ref.beteiligtennummer" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Verweis auf eine durch den Grunddatensatz erfasste Person durch die Beteiligtennummer.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="identifier" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Es liegt keine Beschreibung vor</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="namenszusatzFuerOrganisationen" minOccurs="0" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Es liegt keine Beschreibung vor</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="registereintragung.inso" minOccurs="0">
              <xs:complexType>
                <xs:choice>
                  <xs:element name="inlaendischesRegistergericht">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="registergericht.ort" type="tns:Code.GDS.Gerichte" />
                        <xs:element name="registerart" type="tns:Code.INSO.Registerart" />
                        <xs:element name="registernummer" type="xoev-lc:String.Latin">
                          <xs:annotation>
                            <xs:documentation>Das Element Registernummer enthält die gesamte Registerbezeichnung, wenn das Element Registerart leer ist.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="sonstigeRegisterbehoerde">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="registerbehoerde" type="xoev-lc:String.Latin">
                          <xs:annotation>
                            <xs:documentation>Hier wird eine Langbezeichnung  der Registerbehörde übergeben, wenn diese nicht ein inländisches Registergericht ist.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="registerbezeichnung" type="xoev-lc:String.Latin">
                          <xs:annotation>
                            <xs:documentation>Dieses Element enthält die Bezeichnung in dem Register.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                </xs:choice>
              </xs:complexType>
            </xs:element>
            <xs:element name="ansprechpartner" minOccurs="0" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Maximal 50 Zeichen.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="handelndUnter" minOccurs="0" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>z.B. handelnd unter "Atelier Peter Müller, Efeuweg 4, 12345 Irgendwo" (maximal 255 Zeichen)</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="zustellungsart" type="tns:Code.INSO.Zustellungsart">
              <xs:annotation>
                <xs:documentation>Zulässige Werte sind definiert im Schema xj_0320_wl_insolvenz_x_x.xsd (Vorschlagswert des Verwalters für das Gericht).</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="uebertragungsweg" type="tns:Code.INSO.Uebertragungsweg">
              <xs:annotation>
                <xs:documentation>Zulässige Werte sind definiert im Schema xj_0320_wl_insolvenz_x_x.xsd (Vorschlagswert des Verwalters für das Gericht).</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="postempfaenger" type="xs:boolean">
              <xs:annotation>
                <xs:documentation>Angabe, ob der Gläubiger Postempfänger ist (Vorschlagswert des Verwalters für das Gericht)</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="glaeubigermerkmale" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Es liegt keine Beschreibung vor</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="masseglaeubiger" type="xs:boolean">
                    <xs:annotation>
                      <xs:documentation>Angabe ob der Gläubiger Massegläubiger ist</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="neumasseglaeubiger" type="xs:boolean">
                    <xs:annotation>
                      <xs:documentation>Angabe ob der Gläubiger Neumassegläubiger ist</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="altmasseglaeubiger" type="xs:boolean">
                    <xs:annotation>
                      <xs:documentation>Angabe ob der Gläubiger Altmassegläubiger ist</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="absonderungsglaeubiger" type="xs:boolean">
                    <xs:annotation>
                      <xs:documentation>Angabe ob der Gläubiger Absonderungsgläubiger ist</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="nachrangglaeubiger" type="xs:boolean">
                    <xs:annotation>
                      <xs:documentation>Angabe ob der Gläubiger Nachranggläubiger ist</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="forderung" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Es liegt keine Beschreibung vor</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="ref.rollennummer" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Hier kann ein Bezug zu einer Rolle angegeben werden. Im vorliegenden Feld wird dann vermerkt, für welchen Beteiligten (ggf. auch in welcher Rolle ) die Forderung besteht.  Für jede Rolle, auf die verwiesen wird, muss ein entsprechendes Element "Beteiligung/Rolle" vorhanden sein. Verwiesen wird auf die Rollennummer.</xs:documentation>
              </xs:annotation>
            </xs:element>
