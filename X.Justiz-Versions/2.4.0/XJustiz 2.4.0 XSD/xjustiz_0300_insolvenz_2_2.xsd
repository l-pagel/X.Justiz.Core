              </xs:annotation>
            </xs:element>
            <xs:element name="anteil">
              <xs:annotation>
                <xs:documentation>Anteil am Vermögensträger</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="betrag" type="tns:Type.GDS.Geldbetrag">
                    <xs:annotation>
                      <xs:documentation>Höhe des Anteils</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="artDesAnteilsrechts" type="xoev-lc:String.Latin">
                    <xs:annotation>
                      <xs:documentation>z.B. Gesellschaftsanteil, gezeichnetes Kapital etc.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
