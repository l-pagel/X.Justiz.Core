<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://www.xjustiz.de" xmlns:xdo="http://www.xdomea.de" xmlns:xoev-lc="http://xoev.de/latinchars/1_1/datatypes" targetNamespace="http://www.xjustiz.de" version="2.0.0" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xs:include schemaLocation="xjustiz_0000_grunddatensatz_2_1.xsd" />
  <xs:include schemaLocation="xjustiz_0710_cl_grundbuch_2_0.xsd" />
  <xs:include schemaLocation="xjustiz_0720_cl_rollenbezeichnung_grundbuch_2_0.xsd" />
  <xs:import schemaLocation="xdomea_typen_1_0.xsd" namespace="http://www.xdomea.de" />
  <xs:import schemaLocation="latinchars.xsd" namespace="http://xoev.de/latinchars/1_1/datatypes" />
  <xs:complexType name="Type.GRBU.Berechtigter">
    <xs:choice>
      <xs:element name="eigentuemer_Rechtsinhaber" type="tns:Type.GRBU.Person_oder_Gesamthandsgemeinschaft" />
      <xs:element name="anderer_Bestand_berechtigt" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GRBU.Ref.Bestand" />
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="Type.GRBU.Bestandsverzeichnis">
    <xs:sequence>
      <xs:element name="bestand" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="bestandsnummer" minOccurs="0" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Eindeutige Nummer zur Referenzierung</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="grundbuchamt" type="tns:Code.GDS.Gerichte">
              <xs:annotation>
                <xs:documentation>Amtsgericht oder sonstige Stelle, bei der das Grundbuchblatt geführt wird</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="grundbuchbezirk" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Der Grundbuchbezirk ist auf dem Grundbuchblatt in der Regel mit der Einleitung "Grundbuch von" oder "Grundbuch für" angegeben.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="blattstelle" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Nr. des Grundbuchblatts</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="lfd_Nr" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Jedes Grundstück ist im Bestandsverzeichnis unter einer eigenen lfd.Nr. gebucht</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="auswahl_Baurecht" minOccurs="0">
              <xs:complexType>
                <xs:choice>
                  <xs:element name="erbbaurecht" type="tns:Type.GRBU.Erbbaurecht" />
                  <xs:element name="grundstueck" type="tns:Type.GRBU.Grundstueck" />
                  <xs:element name="sonstige" type="xoev-lc:String.Latin">
                    <xs:annotation>
                      <xs:documentation>Z.B. Bergwerks, Bahn- und Fischereigrundbücher.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="aktivvermerke" type="xoev-lc:String.Latin">
                    <xs:annotation>
                      <xs:documentation>Herrschvermerk.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="wohnungs_Teileigentum">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="grundstueck" type="tns:Type.GRBU.Grundstueck" />
                        <xs:element name="groesse_Miteigentumsanteil" type="xoev-lc:String.Latin">
                          <xs:annotation>
                            <xs:documentation>Angabe des Bruchteils</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="bezeichnung_des_Sondereigentums" maxOccurs="unbounded" type="xoev-lc:String.Latin">
                          <xs:annotation>
                            <xs:documentation>Beliebige Bezeichnung z.B.als Wohnung, Büro,Gewerbefläche (beispielsweise Praxis usw.) o.ä.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="sondernutzungsrecht" minOccurs="0" maxOccurs="unbounded" type="xoev-lc:String.Latin">
                          <xs:annotation>
                            <xs:documentation>Inhalt der Sondernutzungsrechte am gemeinschaftlichen Eigentum.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="zustimmungserfordernis" minOccurs="0" type="xoev-lc:String.Latin">
                          <xs:annotation>
                            <xs:documentation>Z.B. des Verwalters bei Veräußerung. Auch Ausnahmen können hier angegeben werden.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="wohnungserbbaurecht_Teilerbbaurecht">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="erbbaurecht" type="tns:Type.GRBU.Erbbaurecht" />
                        <xs:element name="groesse_Miteigentumsanteil" type="xoev-lc:String.Latin">
                          <xs:annotation>
                            <xs:documentation>Angabe des Bruchteils</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="bezeichnung_des_Sondereigentums" maxOccurs="unbounded" type="xoev-lc:String.Latin" />
                        <xs:element name="sondernutzungsrecht" minOccurs="0" maxOccurs="unbounded" type="xoev-lc:String.Latin" />
                        <xs:element name="zustimmungserfordernis" minOccurs="0" type="xoev-lc:String.Latin">
                          <xs:annotation>
                            <xs:documentation>Z.B. Verwalter bei Veräußerung.  Auch Ausnahmen können hier angegeben werden.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                </xs:choice>
              </xs:complexType>
            </xs:element>
            <xs:element name="miteigentumsanteil" minOccurs="0">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="bezeichnung" type="xoev-lc:String.Latin" />
                  <xs:element name="hoehe" type="xoev-lc:String.Latin">
                    <xs:annotation>
                      <xs:documentation>Hoehe des MA</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="gemeinde" minOccurs="0" type="xoev-lc:String.Latin" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GRBU.Beurkundung_Grundbuch">
    <xs:complexContent>
      <xs:extension base="tns:Type.GDS.Beurkundung">
        <xs:sequence>
          <xs:element name="beteiligter" minOccurs="0" maxOccurs="unbounded">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="betroffener_Beteiligter" type="tns:Type.GDS.Ref.Rollennummer" />
                <xs:element name="vollmacht" minOccurs="0" type="xoev-lc:String.Latin" />
                <xs:element name="identifikation" minOccurs="0" type="xoev-lc:String.Latin">
                  <xs:annotation>
                    <xs:documentation>Feststellung der Personenechtheit, z.B. Reisepass.</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="ref_Dok_ID" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Dieses Element enthält einen Verweis auf die ID eines Dokuments, das im Grunddatensatz in der Liste der (übermittelten) Dokumente aufgeführt ist.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:complexContent>
                <xs:extension base="xdo:T_Identifizierung" />
              </xs:complexContent>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="Type.GRBU.Erbbaurecht">
    <xs:sequence>
      <xs:element name="bezeichnung_Als_Erbbaurecht" type="xoev-lc:String.Latin" />
      <xs:element name="daten_des_Erbbaugrundstuecks" maxOccurs="unbounded" type="tns:Type.GRBU.Grundstueck" />
      <xs:element name="dauer_Erbbaurecht" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Anzahl der Jahre, für die das Erbbaurecht bestellt wurde.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="zustimmungserfordernis_Grundstueckseigentuemer" minOccurs="0" maxOccurs="unbounded" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Eventuell bestehende Zustimmungserfordernisse des Grundstückseigentümers nach § 5 ErbbauRG.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GRBU.Gesamthandsgemeinschaft">
    <xs:sequence>
      <xs:element name="berechtigungsverhaeltnis" minOccurs="0" type="tns:Code.GRBU.Berechtigungsverhaeltnis.Typ3">
        <xs:annotation>
          <xs:documentation>Zulässige Werte sind definiert in der Codeliste WL_Berechtigungsverhaeltnis.             Bei Alleineigentum kann das Element weggelassen werden.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="eigentuemer_Rechtsinhaber" maxOccurs="unbounded" type="tns:Type.GRBU.Person_oder_Gesamthandsgemeinschaft" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GRBU.Grundstueck">
    <xs:sequence>
      <xs:element name="flurstueck" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="gemarkung" type="xoev-lc:String.Latin" />
            <xs:element name="flur" minOccurs="0" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Flurnummer</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="flurstuecksnummer">
              <xs:annotation>
                <xs:documentation>Flurstücksnummer</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="zaehler" type="xoev-lc:String.Latin" />
                  <xs:element name="nenner" minOccurs="0" type="xoev-lc:String.Latin" />
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="beschrieb" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Wirtschaftsart und Lage des betroffenen Flurstücks</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="wirtschaftsart" maxOccurs="unbounded" type="xoev-lc:String.Latin" />
                  <xs:element name="lage" maxOccurs="unbounded" type="xoev-lc:String.Latin" />
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="groesse" minOccurs="0" type="xoev-lc:String.Latin" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GRBU.Person_oder_Gesamthandsgemeinschaft">
    <xs:sequence>
      <xs:element name="auswahl_Person_Gesamthandsgemeinschaft">
        <xs:complexType>
          <xs:choice>
            <xs:element name="gesamthandsgemeinschaft" type="tns:Type.GRBU.Gesamthandsgemeinschaft" />
            <xs:element name="person">
              <xs:annotation>
                <xs:documentation>Dieses Element enthält einen Verweis auf einen Beteiligten, der in den Grunddaten in der Liste der Beteiligten aufgeführt ist.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="betroffene_Person" type="tns:Type.GDS.Ref.Rollennummer" />
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:choice>
        </xs:complexType>
      </xs:element>
      <xs:element name="anteil" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Bruchteil</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="lfd_Nr" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Die laufende Nummer des Eigentümers in Abt.I des Grundbuchblattes</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GRBU.Rangvorbehalt">
    <xs:sequence>
      <xs:element name="beschreibung" default="Grundpfandrecht" type="xoev-lc:String.Latin" />
      <xs:element name="betrag" type="tns:Type.GDS.Geldbetrag" />
      <xs:element name="zinsen" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="prozentsatz" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Zinssatz in Prozent</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="zinsbeginn" minOccurs="0" type="xoev-lc:String.Latin" />
            <xs:element name="zinszeitraum" minOccurs="0" default="jährlich" type="xoev-lc:String.Latin" />
            <xs:element name="sonstiges" minOccurs="0" type="xoev-lc:String.Latin" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="nebenleistung_wiederkehrend" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>In der Regel Verzinsung des Nennbetrages eines Grundpfandrechtes</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="auswahl_Pozentsatz_Betrag">
              <xs:complexType>
                <xs:choice>
                  <xs:element name="prozentsatz" type="xoev-lc:String.Latin">
                    <xs:annotation>
                      <xs:documentation>Zinssatz in Prozent</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="betrag" type="tns:Type.GDS.Geldbetrag">
                    <xs:annotation>
                      <xs:documentation>Als Währungsbezeichnung sind die dreistelligen Buchstaben-Abkürzungen gemäss ISO 4217 zu verwenden.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:choice>
              </xs:complexType>
            </xs:element>
            <xs:element name="beginn" minOccurs="0" type="xoev-lc:String.Latin" />
            <xs:element name="zeitraum" minOccurs="0" default="jährlich" type="xoev-lc:String.Latin" />
            <xs:element name="sonstiges" minOccurs="0" type="xoev-lc:String.Latin" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="auswahl_Nebenleistung_einmalig" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Einmalige Nebenleistung eines Grundpfandrechts</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice>
            <xs:element name="betrag" type="tns:Type.GDS.Geldbetrag">
              <xs:annotation>
                <xs:documentation>Als Währungsbezeichnung sind die dreistelligen Buchstaben-Abkürzungen gemäss ISO 4217 zu verwenden.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="prozentsatz" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Zinssatz in Prozent</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
        </xs:complexType>
      </xs:element>
      <xs:element name="anfangszeit" type="xoev-lc:String.Latin" />
      <xs:element name="einmalig" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>Beschränkung des Rangvorbehalts auf einen Ausübungsfall.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GRBU.Recht">
    <xs:sequence>
      <xs:element name="rechtenummer" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Eindeutige Nummer zur Referenzierung</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="betroffener_Bestand" maxOccurs="unbounded" type="tns:Type.GRBU.Ref.Bestand">
        <xs:annotation>
          <xs:documentation>Betroffener Bestand</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="eintragungsgrundlage" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GRBU.Beurkundung_Grundbuch" />
      <xs:element name="auswahl_Abteilung">
        <xs:complexType>
          <xs:choice>
            <xs:element name="abteilungI">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="eigentuemer_Rechtsinhaber" maxOccurs="unbounded" type="tns:Type.GRBU.Person_oder_Gesamthandsgemeinschaft" />
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="abteilungII">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="lfd_Nr" minOccurs="0" type="xoev-lc:String.Latin">
                    <xs:annotation>
                      <xs:documentation>Die laufende Nummer des Rechts in Abt. II des Grundbuchblattes</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="belastungsart_AbtII" minOccurs="0" type="tns:Code.GRBU.Belastungsart.AbtII.Typ3">
                    <xs:annotation>
                      <xs:documentation>Zulässige Werte sind definiert in der Codeliste WL_Belastungsart_AbtII.
Vormerkungen (mit Ausnahme der Auflassungsvormerkung/ Rückauflassungsvormerkung und der Vormerkung auf Bildung von Sondereigentum) können mit Hilfe des Elements Ist_Vormerkung realisiert werden. Bei Belastungsarten, die nicht in der Liste enthalten sind, soll der Listeneintrag Sonstiges gewählt und die Belastung im Feld Bemerkung näher spezifiziert werden.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="bemerkung" minOccurs="0" type="xoev-lc:String.Latin">
                    <xs:annotation>
                      <xs:documentation>Schlagwort zur näheren Spezifikation der Belastungart: für Belastungsarten, die eigentlich Oberbegriffe sind.                     Beispiel: Bei der Belasungsart "beschränkte persönliche Dienstbarkeit" kann als Bemerkung "Geh- und Fahrrecht"                     angegeben werden.                     Bei Belastungsarten, die nicht in der Codeliste vorkommen, kann unter Belastungsart_AbtII der Wert "Sonstiges"                     angegeben und hier eine nähere Angabe über die Art der Belastung eingetragen werden.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="spezifizierung_der_Belastung" minOccurs="0" type="xoev-lc:String.Latin">
                    <xs:annotation>
                      <xs:documentation>Deckt die Fälle Teilflächenbelastung (Flurstücksbelastung oder §7 GBO),                      Quotennießbrauch sowie Nießbrauch eines ideellen Miteigentumsanteils ab.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="anteilsbelastung" minOccurs="0">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="betroffener_Bestand" type="tns:Type.GRBU.Ref.Bestand">
                          <xs:annotation>
                            <xs:documentation>Zur Referenzierung auf die jeweilige Bestandsnummer</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="ref_Nr_Abteilung_I" type="tns:Type.GRBU.Ref.AbteilungI">
                          <xs:annotation>
                            <xs:documentation>Zur Referenzierung auf einen Eintrag in Abteilung I</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="ist_Vormerkung" minOccurs="0" type="xs:boolean">
                    <xs:annotation>
                      <xs:documentation>Nach § 883 BGB oder ggf. § 18 Abs. 2 GBO bei bestehendem Recht.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="auswahl_Berechtigter" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GRBU.Berechtigter" />
                  <xs:element name="rangvermerk" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                      <xs:documentation>Regelt das Rangverhältnis zu anderen Rechten in Abt. II und Abt. III des Grundbuches.                     Hier wird sowohl die Rangänderung zwischen mehreren bestehenden Rechten als auch die Rangänderung zwischen einem bestehenden und einem neu einzutragenden Recht abgedeckt.                     Standardtext: Das Recht hat Vorrang/Gleichrang/Nachrang gegenüber dem Recht Abteilung XXX Nr. 00.</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="art_des_Ranges" maxOccurs="unbounded">
                          <xs:complexType>
                            <xs:sequence>
                              <xs:element name="beschreibung" type="xoev-lc:String.Latin">
                                <xs:annotation>
                                  <xs:documentation>Freitextfeld zur Darstellung von Informationen, die nicht strukturiert abbildbar sind. Hier kann z.Bsp. auch eingetragen werden "unter Ausnutzung / unter teilweiser Ausnutzung des Rangvorbehaltes".</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                              <xs:element name="betroffenes_Recht" maxOccurs="unbounded" type="tns:Type.GRBU.Ref.Recht">
                                <xs:annotation>
                                  <xs:documentation>Zur Referenzierung auf ein oder mehrere bestehende Rechte</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                            </xs:sequence>
                          </xs:complexType>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="wirksamkeitsvermerk" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                      <xs:documentation>Vermerk, dass das Recht gegenüber einer vorrangig eingetragenen Vormerkung oder einer zeitlich früher eingetragenen relativen Verfügungsbeschränkung (z.B. Nacherbenvernerk) wirksam ist.                     Hier wird sowohl die Wirksamkeit zwischen mehreren bestehenden Rechten als auch die Wirksamkeit zwischen einem bestehenden und einem neu einzutragenden Recht abgedeckt.                     Standardtext: Das Recht ist wirksam gegenüber dem Recht Abteilung XXX Nr. 00.</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="beschreibung" type="xoev-lc:String.Latin">
                          <xs:annotation>
                            <xs:documentation>Freitextfeld zur Darstellung von Informationen, die nicht strukturiert abbildbar sind..</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="betroffenes_Recht" maxOccurs="unbounded" type="tns:Type.GRBU.Ref.Recht">
                          <xs:annotation>
                            <xs:documentation>Zur Referenzierung auf ein oder mehrere bestehende Rechte</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="veraenderung" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                      <xs:documentation>Beispiel Verpfändung einer Auflassungsvormerkung</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:sequence>
