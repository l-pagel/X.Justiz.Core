<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://www.xjustiz.de" xmlns:xdo="http://www.xdomea.de" xmlns:xdomea="http://www.xdomea.de/V2.2.0" xmlns:xoev-lc="http://xoev.de/latinchars/1_1/datatypes" targetNamespace="http://www.xjustiz.de" version="2.2.1" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xs:include schemaLocation="xjustiz_0010_cl_allgemein_2_2.xsd" />
  <xs:include schemaLocation="xjustiz_0020_cl_gerichte_2_3.xsd" />
  <xs:include schemaLocation="xjustiz_0030_cl_rechtsform_2_1.xsd" />
  <xs:include schemaLocation="xjustiz_0040_cl_rollenbezeichnung_2_0.xsd" />
  <xs:include schemaLocation="xjustiz_0050_cl_staaten_2_1.xsd" />
  <xs:include schemaLocation="xjustiz_0060_cl_telekommunikation_2_1.xsd" />
  <xs:include schemaLocation="xjustiz_0070_cl_justizvollzugsanstalt_2_0.xsd" />
  <xs:include schemaLocation="xjustiz_0080_cl_registerzeichen_2_1.xsd" />
  <xs:include schemaLocation="xjustiz_0090_cl_bnotk_2_0.xsd" />
  <xs:include schemaLocation="xjustiz_0095_cl_personalstatut_2_0.xsd" />
  <xs:import schemaLocation="xdomea_typen_1_0.xsd" namespace="http://www.xdomea.de" />
  <xs:import schemaLocation="xdomea-Datentypen.xsd" namespace="http://www.xdomea.de/V2.2.0" />
  <xs:import schemaLocation="xdomea-Baukasten.xsd" namespace="http://www.xdomea.de/V2.2.0" />
  <xs:import schemaLocation="latinchars.xsd" namespace="http://xoev.de/latinchars/1_1/datatypes" />
  <xs:complexType name="Type.GDS.Akte">
    <xs:sequence>
      <xs:element name="identifikation" type="tns:Type.GDS.Identifikation" />
      <xs:element name="aktentyp" type="tns:Code.GDS.Aktentyp" />
      <xs:element name="anzeigename" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Freitextfeld für ergänzende Angaben zum Aktentyp</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="aktenzeichen" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="az_Inhalt" type="xoev-lc:String.Latin" />
            <xs:choice minOccurs="0">
              <xs:element name="az_Gericht" minOccurs="0" type="tns:Code.GDS.Gerichte" />
              <xs:element name="az_Sonstige" minOccurs="0" type="xoev-lc:String.Latin" />
            </xs:choice>
            <xs:element name="az_Art" minOccurs="0" type="tns:Code.GDS.Aktenzeichenart" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="weiteres_Ordnungkriterium_der_Behoerde" minOccurs="0" type="xoev-lc:String.Latin" />
      <xs:element name="person" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="herkunftsland" minOccurs="0" type="tns:Code.GDS.Staaten" />
            <xs:element name="vollerName" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Voller_Name" />
            <xs:element name="geburt" minOccurs="0" type="tns:Type.GDS.Geburt" />
            <xs:element name="staatsangehoerigkeit" minOccurs="0" maxOccurs="unbounded" type="tns:Code.GDS.Staaten" />
            <xs:element name="sprache" minOccurs="0" maxOccurs="unbounded" type="xoev-lc:String.Latin" />
            <xs:element name="alias" minOccurs="0" maxOccurs="unbounded">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="vollerName" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Voller_Name" />
                  <xs:element name="geburt" minOccurs="0" type="tns:Type.GDS.Geburt" />
                  <xs:element name="staatsangehoerigkeit" minOccurs="0" maxOccurs="unbounded" type="tns:Code.GDS.Staaten" />
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="erstellungszeitpunkt_der_Akte_fuer_den_Versand" minOccurs="0" type="xs:dateTime" />
      <xs:element name="aktenreferenzen" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="aktenreferenzart" type="tns:Code.GDS.Aktenreferenzart" />
            <xs:element name="id_der_referenzierten_Akte" type="xdomea:stringUUIDType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="abgebende_Stelle" minOccurs="0" type="tns:Code.GDS.Gerichte" />
      <xs:element name="inhalt" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="teilakte" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Teilakte" />
            <xs:element name="dokument" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Dokument" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="anwendungsspezifische_Erweiterung" minOccurs="0" maxOccurs="unbounded" type="xdomea:AnwendungsspezifischeErweiterungType" />
      <xs:element name="ruecksendung_EEB_erforderlich" default="false" type="xs:boolean" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Aktenzeichen">
    <xs:sequence>
      <xs:element name="sachgebietsschluessel" minOccurs="0" type="xoev-lc:String.Latin" />
      <xs:element name="zusatzkennung" minOccurs="0" type="xoev-lc:String.Latin" />
      <xs:element name="abteilung" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Abteilungs-/Dezernats-/Geschäftsstellenbezeichnung</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="register" type="tns:Code.GDS.Registerzeichen" />
      <xs:element name="laufendeNummer" type="xoev-lc:String.Latin" />
      <xs:element name="jahr" type="xoev-lc:String.Latin" />
      <xs:element name="vorsatz" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Ein optionaler Vorsatz für das Aktenzeichen, z.B. für die Mitbeschuldigtenkennzeichnung</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="zusatz" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Ein optionaler Zustz für das Aktenzeichen</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="dezernat" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>zusätzliche Dezernatskennung, falls das Dezernat von der Abteilung abweicht.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="erfassungsdatum" minOccurs="0" type="tns:Type.GDS.Datumsangabe" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Aktenzeichenmitteilung.Justiz_zu_Extern">
    <xs:sequence>
      <xs:element name="nachrichtenkopf">
        <xs:complexType>
          <xs:complexContent>
            <xs:extension base="tns:Type.GDS.Nachrichtenkopf.Justiz_zu_Extern">
              <xs:sequence>
                <xs:element name="ereignis" type="tns:Code.GDS.Ereignis.Aktenzeichenmitteilung" />
                <xs:element name="ref.instanznummer.absender" type="xoev-lc:String.Latin">
                  <xs:annotation>
                    <xs:documentation>Dieses Element enthält einen Verweis auf die Instanz. Verwiesen wird auf das Element Instanznummer. Es ist die Instanz gemeint, in der der Absender beschrieben ist.</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="ref.instanznummer.empfaenger" type="xoev-lc:String.Latin">
                  <xs:annotation>
                    <xs:documentation>Dieses Element enthält einen Verweis auf die Instanz. Verwiesen wird auf das Element Instanznummer. Es ist die Intanz gemeint, in der der Empfänger spezifiziert ist.</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="grunddaten">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="verfahrensdaten">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="instanzdaten" minOccurs="2" maxOccurs="2">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="instanznummer" type="xoev-lc:String.Latin">
                          <xs:annotation>
                            <xs:documentation>Die Instanznummer wird aufsteigend nummeriert, vom Absender zum Empfänger und hat nichts mit dem juristischen Begriff der 1., 2. oder 3. Instanz zu tun.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="sachgebiet" type="tns:Code.GDS.Sachgebiet" />
                        <xs:element name="sachgebietszusatz" minOccurs="0" type="xoev-lc:String.Latin">
                          <xs:annotation>
                            <xs:documentation>Möglichkeit für zusätzliche Angaben bezüglich des Sachgebiets, beispielsweise in der Finanzgerichtsbarkeit die Angabe des Steuerjahres.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="instanzbehoerde" type="tns:Type.GDS.Behoerde" />
                        <xs:element name="verfahrensinstanznummer" minOccurs="0" type="xoev-lc:String.Latin">
                          <xs:annotation>
                            <xs:documentation>Hier wird die Verfahrensinstanznummer der Institution übergeben, sofern es eine solche gibt und diese bekannt ist. Sie dient in diesem Fall dazu, das Verfahren eindeutig zu kennzeichnen (unangängig von unterschiedlichen Schreibweisen von Aktenzeichen).</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="ereignisId" minOccurs="0" type="xoev-lc:String.Latin" />
                        <xs:element name="aktenzeichen" type="xoev-lc:String.Latin">
                          <xs:annotation>
                            <xs:documentation>Bei den Instanzdaten des Gerichts wird hier des Aktenzeichen des Verfahrens übergeben, in dem die Terminladung erstellt wird. In den Instanzdaten der empfangenden Behörde wird deren Aktenzeichen übergeben.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="kurzrubrum" minOccurs="0" type="xoev-lc:String.Latin">
                          <xs:annotation>
                            <xs:documentation>In Instanzdaten des Gerichts wird das vom Fachverfahren erzeugte Kurzrubrum übergeben. In den Instanzdaten des Empfängers wird das Element nicht übergeben.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="beteiligung" minOccurs="0">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="rolle" maxOccurs="unbounded">
                          <xs:annotation>
                            <xs:documentation>Jeder Beteiligte hat eine oder mehrere Rollen, z.B. Kläger, Beklagter, Zeuge, Dolmetscher, Rechtsanwalt.</xs:documentation>
                          </xs:annotation>
                          <xs:complexType>
                            <xs:sequence>
                              <xs:element name="loeschen" minOccurs="0" type="xs:boolean">
                                <xs:annotation>
                                  <xs:documentation>Diese Information bezieht sich auf die Löschung einer Rolle aus dem Verfahren. Wenn dieses Element mit true belegt ist, gibt der Absender der XML Info zu erkennen, dass er die Löschung der Rolle vorschlägt. Es liegt in der Entscheidung des Empfängers, ob diese Daten gelöscht werden oder nicht.</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                              <xs:element name="rollennummer" type="xoev-lc:String.Latin">
                                <xs:annotation>
                                  <xs:documentation>Die Rollennummer wird als eindeutige Kennziffer innerhalb des XML-Dokuments benötigt, um aus anderen Elementen innerhalb des XML-Dokuments heraus auf Beteiligte und Rollen verweisen zu können.  Die Rollennummer kann aus einem Datenbank-Schlüssel abgeleitet werden, der dem Beteiligten in der Datenbank eines Gerichts, einer Staatsanwaltschaft oder einer sonstigen Behörde zur eindeutigen Identifizierung zugeordnet ist. Zwingend erforderlich ist dies nicht. Sofern die einzelnen Kommunikationsteilnehmer intern unterschiedliche Datenbank-Schlüssel verwenden, können diese im Element Rollen_ID (siehe unten) abgelegt werden. Die Rollennummer kann dann frei bestimmt werden, solange sie nur innerhalb eines XML-Dokuments eindeutig bleibt.</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                              <xs:element name="rollen_ID" minOccurs="0" maxOccurs="unbounded">
                                <xs:complexType>
                                  <xs:sequence>
                                    <xs:element name="id" type="xoev-lc:String.Latin">
                                      <xs:annotation>
                                        <xs:documentation>Das String-Feld "ID" ist für die Aufnahme des Primärschlüssels der Rolle der referenzierten Instanz gedacht. Auf diese Weise ist es möglich, bei Kommunikation zwischen verschiedenen Instanzen (wobei auch außergerichtliche Teile des Verfahrens als Instanzen zu verstehen sind (z. Bsp. Vorgänge bei Verwaltungsbehörden oder Polizei)) die Primärschlüssel verschiedenster Anwendungen in verschiedenen Behörden mittels X-Justiz für eine erleichterte automatische Verarbeitung auszutauschen. Die Referenzierung der Instanzdatenist möglich, für verschiedene Instanzen können verschiedene Rollenbezeichnungen für einen Beteiligten mitgeteilt werden.</xs:documentation>
                                      </xs:annotation>
                                    </xs:element>
                                    <xs:element name="ref.instanznummer" type="xoev-lc:String.Latin">
                                      <xs:annotation>
                                        <xs:documentation>Dieses Element enthält einen Verweis auf die Instanz, bei der die oben angegebene ID verwendet wird. Verwiesen wird auf das Element Instanznummer.</xs:documentation>
                                      </xs:annotation>
                                    </xs:element>
                                  </xs:sequence>
                                </xs:complexType>
                              </xs:element>
                              <xs:element name="rollenbezeichnung" type="tns:Code.GDS.Rollenbezeichnung" />
                              <xs:element name="nr" default="1" type="xs:integer">
                                <xs:annotation>
                                  <xs:documentation>Wenn es in einem Verfahren mehrere gleichartige Beteiligte gibt (z. B. mehrere Kläger), sind diese fortlaufend zu numerieren. Jede Kombination aus Rollenbezeichnung und Nummer (z.B. Kläger 1, Kläger 2, Beklagter 1 usw.) darf pro Verfahren nur einmal vorkommen.  Mit XML-Mitteln kann nur die Eindeutigkeit innerhalb des jeweiligen XJustiz-Datensatzes überprüft werden.  Für die überprüfung der verfahrensweiten Eindeutigkeit sind die jeweiligen Kommunikationsteilnehmer verantwortlich.</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                              <xs:element name="geschaeftszeichen" minOccurs="0" type="xoev-lc:String.Latin">
                                <xs:annotation>
                                  <xs:documentation>Hier ist Raum für ein eigenes Geschäftszeichen des Beteiligten, z.B. die Schadensnummer einer Versicherung oder die Mandatsnummer eines Rechtsanwalts.</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                              <xs:element name="referenz" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Ref.Rollennummer">
                                <xs:annotation>
                                  <xs:documentation>Hier kann ein Bezug zu einer anderem Rolle angegeben werden. Einige Rollen haben einen Bezug zu einer anderen Beteiligung. Typisches Beispiel sind Rechtsanwälte.Diese handeln in aller Regel als Prozessbevollmächtigte für einen anderen Beteiligten.Im vorliegenden Feld wird dann vermerkt, für welchen Beteiligten (ggf. auch in welcher Rolle ) der Rechtsanwalt handelt.  Für jede Rolle, auf die verwiesen wird, muss ein entsprechendes Element "Beteiligung/Rolle" vorhanden sein. Verwiesen wird auf die Rollennummer.</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                              <xs:element name="anrede" minOccurs="0" type="xoev-lc:String.Latin">
                                <xs:annotation>
                                  <xs:documentation>Hier können Anredeformen übermittelt werden, die in einem Fachverfahren für den Beteiligten in der betreffenden Rolle gespeichert sind, z.B. "Herrn Rechtsanwalt" oder "Frau Notarin".</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                              <xs:element name="dienstbezeichnung" minOccurs="0" type="xoev-lc:String.Latin">
                                <xs:annotation>
                                  <xs:documentation>Das Element Dienstbezeichnung wird z.B. gebraucht, um Polizisten als Zeugen korrekt laden zu können.</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                              <xs:element name="funktionsbezeichnung" minOccurs="0" type="xoev-lc:String.Latin">
                                <xs:annotation>
                                  <xs:documentation>Hier können Funktionsbezeichnungen übergeben werden, die eine Person näher beschreiben, wobei nicht ihre Rolle im gerichtlichen Verfahren gemeint ist.</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                            </xs:sequence>
                          </xs:complexType>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="herstellerinformation" minOccurs="0" type="tns:Type.GDS.Herstellerinformation" />
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="sendungsdaten">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="papiervorgang" default="false" type="xs:boolean">
                    <xs:annotation>
                      <xs:documentation>Hier wird stets "false" übergeben, da kein Dokument verschickt wird (weder als Anhang in dieser Nachricht, noch per Papierform).</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="xjustizVersion" type="xoev-lc:String.Latin" use="required" fixed="2.2.1">
            <xs:annotation>
              <xs:documentation>Dieses Attribut wird verwendet,um sicherzustellen, dass jedes Instanzdokument gegen die "richtige" Version des XML-Schemas geprüft wird. Angegeben werden hier alle drei Stellen der Versionsnummer. In der Dateibezeichnung sind nur die ersten beiden Stellen aufgeführt, dies ermöglicht es, an einzelnen Dateien Fehlerbereinigungen und dergleichen vorzunehmen, ohne dass die anderen Bestandteile von XJustiz geändert werden müssen.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Aktenzeichenmitteilung.Justiz_zu_Justiz">
    <xs:sequence>
      <xs:element name="nachrichtenkopf">
        <xs:complexType>
          <xs:complexContent>
            <xs:extension base="tns:Type.GDS.Nachrichtenkopf.Justiz_zu_Justiz">
              <xs:sequence>
                <xs:element name="ereignis" type="tns:Code.GDS.Ereignis.Aktenzeichenmitteilung" />
              </xs:sequence>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="grunddaten">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="verfahrensdaten">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="instanzdaten" minOccurs="2" maxOccurs="2" type="tns:Type.GDS.Instanzdaten.Justiz.Erweitert" />
                  <xs:element name="herstellerinformation" minOccurs="0" type="tns:Type.GDS.Herstellerinformation" />
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="sendungsdaten">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="papiervorgang" default="false" type="xs:boolean">
                    <xs:annotation>
                      <xs:documentation>Hier wird stets "false" übergeben, da kein Dokument verschickt wird (weder als Anhang in dieser Nachricht, noch per Papierform).</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="xjustizVersion" type="xoev-lc:String.Latin" use="required" fixed="2.2.1">
            <xs:annotation>
              <xs:documentation>Analog zum statischen Fachdatensatz erhalten die Grunddaten von Nachrichten ein Versionsattribut.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Anrede">
    <xs:choice>
      <xs:element name="anrede.partei" type="tns:Code.GDS.Anrede.Partei">
        <xs:annotation>
          <xs:documentation>Dieses Element ist zu verwenden, wenn das Element "Rollenbezeichnung" nicht den Wert "Prozessbevollmächtigte(r)" hat.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="anrede.pv" type="tns:Code.GDS.Anrede.PV">
        <xs:annotation>
          <xs:documentation>Dieses Element ist zu verwenden, wenn das Element "Rollenbezeichnung" den Wert "Prozessbevollmächtigte(r)" hat.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Anschrift">
    <xs:sequence>
      <xs:element name="anschriftstyp" type="tns:Code.GDS.Anschriftstyp">
        <xs:annotation>
          <xs:documentation>Über dieses Element wird die Art der Adresse angegeben.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="anschriftenzusatz" minOccurs="0" maxOccurs="unbounded" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Hier ist Raum für Adressierungszusätze, z.B. Name des Wohnungsgebers.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="strasse" minOccurs="0" type="xoev-lc:String.Latin" />
      <xs:element name="hausnummer" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Die Hausnummer ist ggf. einschliesslich Buchstaben oder dergleichen anzugeben.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="postfachnummer" minOccurs="0" type="xoev-lc:String.Latin" />
      <xs:element name="postleitzahl" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Bei ausländischen Adressen hier bitte keine Länderkennungskürzel eintragen. Wegen der Schreibweise ausländischer Orte vgl. die Anmerkung zum Element "Ort".</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ort" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Bei postalischen Anschriften kann der hier angegebene Ort von der politischen Gemeinde abweichen. In diesem Fall sollten die postalische und die Lieferadresse separat mitgeteilt werden. Bei ausländischen Adressen empfiehlt die Deutsche Post AG, den Ort möglichst in der Landessprache zu schreiben. Dies erscheint auch im vorliegenden Zusammenhang sinnvoll. Die Deutsche Post AG empfiehlt ferner,  ausländische Ortsnamen in Großbuchstaben anzugeben. Hiervon sollte bei der Übermittlung von XJustiz-Daten möglichst Abstand genommen werden.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ortsteil" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Zusatz, nicht für postalische Zwecke zu verwenden.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="staat" minOccurs="0" type="tns:Code.GDS.Staaten" />
      <xs:element name="erfassungsdatum" minOccurs="0" type="xs:date">
        <xs:annotation>
          <xs:documentation>Datum der letzten Prüfung der Adresse.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ort.unbekannt" minOccurs="0" default="true" type="xs:boolean" />
      <xs:element name="postleitzahl.unbekannt" minOccurs="0" default="true" type="xs:boolean" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Anschrift.Gerichtsfach">
    <xs:annotation>
      <xs:documentation>Diese Daten werden benötigt, wenn für den Beteiligten bei einem Gericht ein besonderes Postabholfach eingerichtet ist. Das Abholfach muss nicht unbedingt bei dem Gericht eingerichtet sein, bei dem das Verfahren anhängig ist. Für die Angabe des Gerichts kann auch hier der oben erwähnte numerische Schlüssel verwendet werden.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="gericht" type="tns:Code.GDS.Gerichte" />
      <xs:element name="fach" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Hier steht die Nummer oder eine andere Bezeichnung des Gerichtsfachs.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Bankverbindung">
    <xs:annotation>
      <xs:documentation>Hier steht die allgemeine Bankverbindung</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="bank" type="xoev-lc:String.Latin" />
      <xs:element name="bankleitzahl" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Bei ausländischen Bankverbindungen wird hier anstelle der deutschen Bankleitzahl der internationale Bank Identifier Code (BIC) angegeben.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="kontoinhaber" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Der Name braucht nur angegeben werden, sofern der Kontoinhaber nicht mit dem Beteiligten identisch ist.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="kontonummer" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Bei ausländischen Bankverbindungen wird hier die International Bank Accounting Number (IBAN) angegeben.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="staat" minOccurs="0" type="tns:Code.GDS.Staaten" />
      <xs:element name="einzugsermaechtigung" minOccurs="0" default="false" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>Liegt eine Einzugsermächtigung für das Gericht vor, bei dem das Verfahren anhängig ist?</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="bankverbindungsnummer" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Zur Referenzierung der Bankverbindung. Neues Feld seit XJustiz 1.4.1</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="iban" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>IBAN der Bankverbindung</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="bic" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>BIC der Bankverbindung</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Bankverbindung.Neu">
    <xs:annotation>
      <xs:documentation>Hier steht die allgemeine Bankverbindung</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="bank" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Der Name der Bank.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="kontoinhaber" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Der Name braucht nur angegeben werden, sofern der Kontoinhaber nicht mit dem Beteiligten identisch ist.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="iban" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>IBAN der Bankverbindung</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="bic" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>BIC der Bankverbindung</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="staat" minOccurs="0" type="tns:Code.GDS.Staaten" />
      <xs:element name="sepa-mandat" minOccurs="0" default="false" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>Liegt ein SEPA-Mandat für das Gericht vor, bei dem das Verfahren anhängig ist?</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="bankverbindungsnummer" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Zur Referenzierung der Bankverbindung. Neues Feld seit XJustiz 1.4.1</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Behoerde">
    <xs:choice>
      <xs:element name="gericht" minOccurs="0" type="tns:Code.GDS.Gerichte" />
      <xs:element name="behoerde" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Für zukünftig vorliegende Wertelisten von Behörden.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="sonstige" type="tns:Type.GDS.Ref.Rollennummer">
        <xs:annotation>
          <xs:documentation>Dieses Element enthält einen Verweis auf eine Beteilgung (Behörde). Verwiesen wird auf den Wert der Rollennummer in "Beteiligung/Rolle". Für jeden Beteiligten, auf den verwiesen wird, muss ein entsprechendes Element "Beteiligung" vorhanden sein.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Behoerde.Justiz">
    <xs:sequence>
      <xs:element name="gericht" minOccurs="0" type="tns:Code.GDS.Gerichte">
        <xs:annotation>
          <xs:documentation>Hier muss ein Wert aus der Codeliste WL_Gerichte angegeben werden.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Beteiligter">
    <xs:sequence>
      <xs:element name="beteiligtennummer" minOccurs="0" type="xoev-lc:String.Latin" />
      <xs:choice>
        <xs:element name="ra.kanzlei" type="tns:Type.GDS.RA.Kanzlei">
          <xs:annotation>
            <xs:documentation>Achtung, dieses Element als dritte Auswahlmöglichkeit neben Natuerliche_Person und Organisation wurde mit XJustiz Version 1.8 neu eingeführt. Vor der Verwendung sollte mit den Kommunikationspartnern abgesprochen werden, ob das empfangende Fachverfahren dies bereits unterstützt.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="natuerlichePerson" type="tns:Type.GDS.NatuerlichePerson" />
        <xs:element name="organisation" type="tns:Type.GDS.Organisation">
          <xs:annotation>
            <xs:documentation>Hier werden alle Beteiligten aufgeführt, die nicht mit dem Typ Natürliche_Person abgedeckt werden können: Gesellschaften, Vereine, Bruchteilsgemeinschaften etc. Die Abgrenzung erfolgt nicht nach juristischen Kategorien, sondern nach datentechnischer Zweckmäßigkeit.</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Beteiligung">
    <xs:sequence>
      <xs:element name="rolle" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Jeder Beteiligte hat eine oder mehrere Rollen, z.B. Kläger, Beklagter, Zeuge, Dolmetscher, Rechtsanwalt.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="loeschen" minOccurs="0" default="false" type="xs:boolean">
              <xs:annotation>
                <xs:documentation>Diese Information bezieht sich auf die Löschung einer Rolle aus dem Verfahren. Wenn dieses Element mit true belegt ist, gibt der Absender der XML Info zu erkennen, dass er die Löschung der Rolle vorschlägt. Es liegt in der Entscheidung des Empfängers, ob diese Daten gelöscht werden oder nicht.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="rollennummer" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Die Rollennummer wird als eindeutige Kennziffer innerhalb des XML-Dokuments benötigt, um aus anderen Elementen innerhalb des XML-Dokuments heraus auf Beteiligte und Rollen verweisen zu können.  Die Rollennummer kann aus einem Datenbank-Schlüssel abgeleitet werden, der dem Beteiligten in der Datenbank eines Gerichts, einer Staatsanwaltschaft oder einer sonstigen Behörde zur eindeutigen Identifizierung zugeordnet ist. Zwingend erforderlich ist dies nicht. Sofern die einzelnen Kommunikationsteilnehmer intern unterschiedliche Datenbank-Schlüssel verwenden, können diese im Element Rollen_ID (siehe unten) abgelegt werden. Die Rollennummer kann dann frei bestimmt werden, solange sie nur innerhalb eines XML-Dokuments eindeutig bleibt.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="rollenId" minOccurs="0" maxOccurs="unbounded">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="id" type="xoev-lc:String.Latin">
                    <xs:annotation>
                      <xs:documentation>Das String-Feld "ID" ist für die Aufnahme des Primärschlüssels der Rolle der referenzierten Instanz gedacht. Auf diese Weise ist es möglich, bei Kommunikation zwischen verschiedenen Instanzen (wobei auch außergerichtliche Teile des Verfahrens als Instanzen zu verstehen sind (z. Bsp. Vorgänge bei Verwaltungsbehörden oder Polizei)) die Primärschlüssel verschiedenster Anwendungen in verschiedenen Behörden mittels X-Justiz für eine erleichterte automatische Verarbeitung auszutauschen. Die Referenzierung der Instanzdatenist möglich, für verschiedene Instanzen können verschiedene Rollenbezeichnungen für einen Beteiligten mitgeteilt werden.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="ref.instanznummer" type="xoev-lc:String.Latin">
                    <xs:annotation>
                      <xs:documentation>Dieses Element enthält einen Verweis auf die Instanz, bei der die oben angegebene ID verwendet wird. Verwiesen wird auf das Element Instanznummer.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="rollenbezeichnung" type="tns:Code.GDS.Rollenbezeichnung">
              <xs:annotation>
                <xs:documentation>Zulässige Rollenbezeichnungen sind in der Datei xj_0040_wl_rollenbezeichnung_x_x.xsd definiert.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="nr" default="1" type="xs:integer">
              <xs:annotation>
                <xs:documentation>Wenn es in einem Verfahren mehrere gleichartige Beteiligte gibt (z. B. mehrere Kläger), sind diese fortlaufend zu numerieren. Jede Kombination aus Rollenbezeichnung und Nummer (z.B. Kläger 1, Kläger 2, Beklagter 1 usw.) darf pro Verfahren nur einmal vorkommen.  Mit XML-Mitteln kann nur die Eindeutigkeit innerhalb des jeweiligen XJustiz-Datensatzes überprüft werden.  Für die überprüfung der verfahrensweiten Eindeutigkeit sind die jeweiligen Kommunikationsteilnehmer verantwortlich.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="geschaeftszeichen" minOccurs="0" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Hier ist Raum für ein eigenes Geschäftszeichen des Beteiligten, z.B. die Schadensnummer einer Versicherung oder die Mandatsnummer eines Rechtsanwalts.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="referenz" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Ref.Rollennummer">
              <xs:annotation>
                <xs:documentation>Hier kann ein Bezug zu einer anderem Rolle angegeben werden. Einige Rollen haben einen Bezug zu einer anderen Beteiligung. Typisches Beispiel sind Rechtsanwälte.Diese handeln in aller Regel als Prozessbevollmächtigte für einen anderen Beteiligten.Im vorliegenden Feld wird dann vermerkt, für welchen Beteiligten (ggf. auch in welcher Rolle ) der Rechtsanwalt handelt.  Für jede Rolle, auf die verwiesen wird, muss ein entsprechendes Element "Beteiligung/Rolle" vorhanden sein. Verwiesen wird auf die Rollennummer.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="anrede" minOccurs="0" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Hier können Anredeformen übermittelt werden, die in einem Fachverfahren für den Beteiligten in der betreffenden Rolle gespeichert sind, z.B. "Herrn Rechtsanwalt" oder "Frau Notarin".</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="dienstbezeichnung" minOccurs="0" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Das Element Dienstbezeichnung wird z.B. gebraucht, um Polizisten als Zeugen korrekt laden zu können.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="funktionsbezeichnung" minOccurs="0" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Hier können Funktionsbezeichnungen übergeben werden, die eine Person näher beschreiben, wobei nicht ihre Rolle im gerichtlichen Verfahren gemeint ist.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="beteiligter" type="tns:Type.GDS.Beteiligter">
        <xs:annotation>
          <xs:documentation>Ein Beteiligter kann eine NatürlichePerson oder eine Organisation sein.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Beurkundung">
    <xs:sequence>
      <xs:element name="urNr" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Urkundennummer der Urkundsperson oder behördliches Aktenzeichen</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="urkundsdatum" minOccurs="0" type="xs:date" />
      <xs:element name="urkundsperson" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Ref.Rollennummer">
        <xs:annotation>
          <xs:documentation>Person, welche die der Eintragung zugrundeliegende Urkunde erstellt hat. Das ist in der Regel ein Notar, in einzelnen Fällen möglicherweise auch ein Gericht.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="keineBeurkundung" minOccurs="0" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>In manchen Fällen ist keine Beurkundung nötig.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="Type.GDS.Datumsangabe">
    <xs:restriction base="xoev-lc:String.Latin">
      <xs:pattern value="\d{4}((-\d{2}){0,1}-\d{2}){0,1}" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="Type.GDS.Dokument">
    <xs:sequence>
      <xs:element name="identifikation" type="tns:Type.GDS.Identifikation" />
      <xs:element name="dokumententyp" type="tns:Code.GDS.Dokumenttyp" />
      <xs:element name="anzeigename" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Freitextfeld für ergänzende Angaben zum Dokumenttyp</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="akteneinsicht" default="true" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>StV muss auf "Nein" schalten</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="dokumentendatum" minOccurs="0" type="xs:date" />
      <xs:element name="eingangszeitpunkt" minOccurs="0" type="xs:dateTime" />
      <xs:element name="veraktungsdatum" minOccurs="0" type="xs:date" />
      <xs:element name="absenderanzeigename" minOccurs="0" type="xoev-lc:String.Latin" />
      <xs:element name="adressatanzeigename" minOccurs="0" type="xoev-lc:String.Latin" />
      <xs:element name="justizkostenrelevanz" minOccurs="0" default="false" type="xs:boolean" />
      <xs:element name="fremdes_Geschaeftszeichen" minOccurs="0" type="xoev-lc:String.Latin" />
      <xs:element name="verweis" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="verweistyp" minOccurs="0" type="tns:Code.GDS.Verweistyp" />
            <xs:element name="anzeigename_SGO" minOccurs="0" type="xoev-lc:String.Latin" />
            <xs:element name="id_des_SGO_auf_das_verwiesen_wird" minOccurs="0" type="xdomea:stringUUIDType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="datei" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="dateiname" type="xoev-lc:String.Latin" />
            <xs:element name="dateiformat" type="tns:Code.GDS.Dateiformat.Typ3ex" />
            <xs:element name="bestandteil" type="tns:Code.GDS.Bestandteiltyp" />
            <xs:element name="versionsnummer" minOccurs="0" type="xs:integer" />
            <xs:element name="dateiname_der_Bezugsdatei" minOccurs="0" maxOccurs="unbounded" type="xoev-lc:String.Latin" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="anwendungsspezifische_Erweiterung" minOccurs="0" maxOccurs="unbounded" type="xdomea:AnwendungsspezifischeErweiterungType" />
      <xs:element name="ruecksendung_EEB_erforderlich" default="false" type="xs:boolean" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Entscheidung">
    <xs:sequence>
      <xs:element name="dokument.id" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Verweis auf ein Dokument (oder Vorgang oder Akte) von XDOMEA. Sofern andere Elemente auf eine Entscheidung verweisen, erfolgt dieser Verweis ebenfalls auf die DOMEA-ID</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="entscheidungsbehoerde">
        <xs:complexType>
          <xs:complexContent>
            <xs:extension base="tns:Type.GDS.Behoerde">
              <xs:sequence>
                <xs:element name="aktenzeichen" minOccurs="0" type="xoev-lc:String.Latin" />
              </xs:sequence>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="entscheidungsdatum" minOccurs="0" type="xs:date" />
      <xs:element name="zustellung" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="zustellungsempfaenger" type="tns:Type.GDS.Ref.Rollennummer">
              <xs:annotation>
                <xs:documentation>Verweis auf Rollennummer</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="zustellungsdatum" minOccurs="0" type="xs:date" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="rechtskraft" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Rechtskraft" />
      <xs:element name="entscheidungstenor" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Entscheidungstenor" />
      <xs:element name="bezug" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Ein textueller Verweis auf die Entscheidung für interne Referenzierungen kann das Element Dokument/Verweis aus dem Grunddatensatz verwendet werden. Beispiel: Im Falle einer Berufung kann hier ein Verweis auf die ursprüngliche Entscheidung stehen.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Entscheidungstenor">
    <xs:sequence>
      <xs:element name="betroffener" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Ref.Beteiligtennummer">
        <xs:annotation>
          <xs:documentation>Verweis auf eine durch den Grunddatensatz erfasste Person durch die Beteiligtennummer.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ergebnis" minOccurs="0" maxOccurs="unbounded" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Für den Fall der Teilrechtskraft</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="sonstigerEntscheidungsinhalt" minOccurs="0" maxOccurs="unbounded" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Freitext</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="anordnungsinhalt" minOccurs="0" maxOccurs="unbounded" type="xoev-lc:String.Latin" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Fortsetzungsterminsdaten">
    <xs:annotation>
      <xs:documentation>Der Fortsetzungstermin enthält alle Element von "Terminsdaten".  Er bezieht sich über das Datum und die Uhrzeit auf einen Haupttermin</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:Type.GDS.Terminsdaten">
        <xs:sequence>
          <xs:element name="hauptterminsdatum" type="tns:Type.GDS.Datumsangabe">
            <xs:annotation>
              <xs:documentation>Dies ist der Bezug zum Haupttermin. Angegeben durch ein Datum. Datumsfeld gemäss DIN ISO 8601 (yyyy-mm-dd)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:choice>
            <xs:element name="hauptterminsuhrzeit" type="xs:time">
              <xs:annotation>
                <xs:documentation>Uhrzeit gemäss DIN ISO 8601 (hh:mm)</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="hauptterminszeit" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Hier ist Raum für eine textuelle Zeitangabe, z.B. "vormittags", "zwischen 14 und 16 Uhr" etc.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Geburt">
    <xs:sequence>
      <xs:element name="geburtsdatum" minOccurs="0" type="tns:Type.GDS.Datumsangabe">
        <xs:annotation>
          <xs:documentation>Datumsfeld nach ISO 8601 (yyyy-mm-dd) - typisiert mit  Pattern</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="geburtsort" minOccurs="0" type="tns:Type.GDS.Ortsangabe" />
      <xs:element name="geburtsname.mutter" minOccurs="0" type="xoev-lc:String.Latin" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Geldbetrag">
    <xs:sequence>
      <xs:element name="zahl" type="xs:double" />
      <xs:element name="waehrung" default="EUR" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Als Währungsbezeichnung sind die dreistelligen Buchstaben-Abkürzungen gemäss ISO 4217 zu verwenden.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Grunddaten">
    <xs:annotation>
      <xs:documentation>Der Type.GDS.Grunddaten enthält Meta-Informationen zum übersandten Dokument und zu dem zugehörigen Verfahren. Die darin enthaltenen Daten sind für alle Gerichtszweige und für alle Verfahrensarten von Interesse. Zusätzliche oder abgewandelte Datenstrukturen können in einem Fachmodul definiert werden.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="verfahrensdaten">
        <xs:annotation>
          <xs:documentation>Verfahrensdaten nehmen alle Informationen auf, die das jeweilige Verfahren mit sich bringt.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="verfahrensnummer" minOccurs="0" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Die Verfahrensnummer ist eine eindeutige Kennziffer für das gesamte betreffende Verfahren im Sinne eines Primärschlüssels. Bei der Übermittlung verfahrenseinleitender Dokumente ist hier "neu" einzutragen. Wenn das Aktenzeichen aus sonstigen Gründen noch nicht bekannt ist, erhält das Element den Wert "unbekannt".  Das vorliegende Element ist vorgesehen für eine Verfahrensnummer, die für das Verfahren über alle Instanzen hinweg gelten soll. Alternativ oder kumulativ kann dem Verfahren in jeder Instanz eine separate Nummer zugewiesen werden; diese wird im Element Instanzdaten/Verfahrensinstanznummer übermittelt.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="instanzdaten" maxOccurs="unbounded">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="instanznummer" type="xoev-lc:String.Latin">
                    <xs:annotation>
                      <xs:documentation>Mit diesem Element ist nicht die juristische Bezeichnung einer Instanz gemeint. Das Element dient vielmehr als Schlüsselfeld für interne Referenzierungen. Die Instanznummer muss für das aktuelle XML-Instanzdokument eindeutig sein und identifiziert die jeweiligen Instanzdaten. Jedoch sollte auf die zeitliche Abfolge der Instanzen geachtet werden. Handelt die STA vor einem Gericht, erhält die STA 0 und das Gericht 1.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="sachgebiet" type="tns:Code.GDS.Sachgebiet" />
                  <xs:element name="sachgebietszusatz" minOccurs="0" type="xoev-lc:String.Latin">
                    <xs:annotation>
                      <xs:documentation>Möglichkeit für zusätzliche Angaben bezüglich des Sachgebiets beispielsweise in der Finanzgerichtsbarkeit die Angabe des Steuerjahres.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="instanzbehoerde" type="tns:Type.GDS.Behoerde">
                    <xs:annotation>
                      <xs:documentation>Instanzbehörde ist die Institution, bei der das Verfahren in der betreffenden Instanz anhängig gewesen ist. Dies kann ein Gericht oder eine Staatsanwaltschaft sein, aber auch z.B. eine Polizeidienststelle oder eine andere Institution. Hier wird entweder das Gericht mit dessen Gerichtskennzahl angegeben oder eine andere Behörde. Wenn die andere Behörde nicht in einer Liste aufgeführt ist, ist sie komplett unter einer Beteiligung anzugeben. Der Verweis auf diese Beteiligung erfolgt unter der Ref_Instanznummer. (Beispielsweise: Polizei) Eine XML-Beispielsdatei ist unter www.xjustiz.de zu finden.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="abteilung" minOccurs="0" type="xoev-lc:String.Latin">
                    <xs:annotation>
                      <xs:documentation>Hier kann die zuständige Abteilung oder der zuständige Spruchkörper angegeben werden, z.B. "7.Zivilkammer", "Dezernat 77" oder "Inspektion 7".</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="verfahrensinstanznummer" minOccurs="0" type="xoev-lc:String.Latin">
                    <xs:annotation>
                      <xs:documentation>Die Verfahrensinstanznummer ist eine eindeutige Kennziffer für die betreffende Instanz in einem Verfahren im Sinne eines Primärschlüssels. Bei der Übermittlung verfahrenseinleitender Dokumente ist hier "neu" einzutragen.  Wenn das Aktenzeichen aus sonstigen Gründen noch nicht bekannt ist, erhält das Element den Wert "unbekannt".  Das vorliegende Element ist vorgesehen für eine Verfahrensnummer, die nur in der betreffenden Instanz hinweg gelten soll. Alternativ oder kumulativ kann dem Verfahren eine instanzübergreifende Nummer zugewiesen werden; diese wird im Element Verfahrensdaten/Verfahrensnummer übermittelt.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="aktenzeichen" type="xoev-lc:String.Latin">
                    <xs:annotation>
                      <xs:documentation>Hier steht das Aktenzeichen, unter dem die Instanzbehörde (siehe oben) das Verfahren führt. Bei der Übermittlung verfahrenseinleitender Dokumente ist hier "neu" einzutragen. Wenn das Aktenzeichen aus sonstigen Gründen noch nicht bekannt ist, erhält das Element den Wert "unbekannt".</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="kurzrubrum" type="xoev-lc:String.Latin">
                    <xs:annotation>
                      <xs:documentation>Hier wird die schlagwortartige Bezeichnung des Verfahrens angegeben, z.B. "Müller ./. Maier" oder "Strafsache gegen Erich Schulze".</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="verfahrensgegenstand" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Verfahrensgegenstand">
                    <xs:annotation>
                      <xs:documentation>Es soll eine 1:N-Zuordnung Verfahren zu Verfahrensgegenständen möglich sein.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="beteiligung" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Beteiligung">
              <xs:annotation>
                <xs:documentation>Für jede Beteiligung gibt es genau einen Beteiligten. Dieser kann mehrere Rollen haben.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="auswahl_Termin" minOccurs="0" maxOccurs="unbounded">
              <xs:complexType>
                <xs:choice>
                  <xs:element name="terminsdaten" type="tns:Type.GDS.Terminsdaten" />
                  <xs:element name="fortsetzungsterminsdaten" type="tns:Type.GDS.Fortsetzungsterminsdaten" />
                </xs:choice>
              </xs:complexType>
            </xs:element>
            <xs:element name="herstellerinformation" minOccurs="0" type="tns:Type.GDS.Herstellerinformation" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="sendungsdaten">
        <xs:annotation>
          <xs:documentation>Sendungsdaten nehmen alle Informationen auf, die die jeweils vorliegende Sendung betreffen. Sendungsübergreifende Informationen sind in den Verfahrensdaten enthalten.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="sendungsprioritaet" minOccurs="0" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Sendungspriorität enthält Sendungsanmerkungen wie "eilt", "eilt sehr" etc. in Form von freiem Text</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="sendungsinhalt" minOccurs="0" type="tns:Code.GDS.Sendungsinhalt" />
            <xs:element name="bemerkung" minOccurs="0" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Dies ist ein generelles Bemerkungsfeld, das alle Arten an Bemerkungen zur Sendung enhalten kann.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="absenderkennung" minOccurs="0" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Hier wird nicht der gesamte Datensatz eines Beteiligten referenziert, sondern nur eine Angabe eines Absendernamens gemacht. Eventuell können hierzu auch E-Mail-Adresse, der OSCI-Benutzername oder die Postfach-ID einer virtuellen Poststelle verwendet werden.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="empfaengerkennung" minOccurs="0" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Hier wird nicht der gesamte Datensatz eines Beteiligten referenziert, sondern nur eine Angabe eines Empfängernamens gemacht. Eventuell können hierzu auch E-Mail-Adresse, der OSCI-Benutzername oder die Postfach-ID einer virtuellen Poststelle verwendet werden</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="eingangsbestaetigungswunsch" minOccurs="0" default="false" type="xs:boolean">
              <xs:annotation>
                <xs:documentation>Mit diesem Element kann ein Einreicher bei einer Sendung AN das Gericht oder die Staatsanwaltschaft angeben, ob er eine Eingangsbestätigung haben möchte oder nicht. Mit dieser Eingangsbestätigung ist ausdrücklich NICHT das EMPFANGSBEKENNTNIS (EB) gemeint, das ein Anwalt bei Sendungen VOM Gericht abgeben soll. Der Einreicher erhält bei Setzen von "true" eine automatisiert erstellte Bestätigung des Erhalts seiner Sendung durch das adressierte Gericht bzw. die adressierte Staatsanwaltschaft.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="papiervorgang" type="xs:boolean">
              <xs:annotation>
                <xs:documentation>Flag für die Erkennung, ob es sich um einen Papiervorgang in elektronischer Form handelt. Liegt ein Papiervorgang auch elektronisch vor ? J(true)</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:choice minOccurs="0">
              <xs:element name="xdomea" type="xdo:T_XDOMEA" />
              <xs:element name="xdomea.2">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="akte" minOccurs="0" type="xdomea:AkteType" />
                    <xs:element name="dokument" minOccurs="0" type="xdomea:DokumentType" />
                    <xs:element name="vorgang" minOccurs="0" type="xdomea:VorgangType" />
                  </xs:sequence>
                  <xs:attribute name="xdomeaVersion" type="xoev-lc:String.Latin" use="required" fixed="2.2.0" />
                </xs:complexType>
              </xs:element>
            </xs:choice>
            <xs:element name="absendernachricht" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Zusatzinformationen zur Nachricht des Absenders, wenn sich die Nachricht auf mehrere Einzelnachrichten verteilt.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="nachrichtenId" type="xoev-lc:String.Latin">
                    <xs:annotation>
                      <xs:documentation>Eindeutige ID der Nachricht. Alle Einzelnachrichten müssen die gleiche ID besitzen, damit eine Zusammengehörigkeit hergestellt werden kann.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="nachrichtenanzahl" type="xs:integer">
                    <xs:annotation>
                      <xs:documentation>Anzahl der einzelnen Nachrichten, aus der sich die gesamte Nachricht zusammensetzt.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="nachrichtenNr" type="xs:integer">
                    <xs:annotation>
                      <xs:documentation>Nummer der aktuellen Einzelnachricht.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="xjustizVersion" type="xoev-lc:String.Latin" use="required" fixed="2.2.1">
      <xs:annotation>
        <xs:documentation>Dieses Attribut wird verwendet,um sicherzustellen, dass jedes Instanzdokument gegen die "richtige" Version des XML-Schemas geprüft wird. Angegeben werden hier alle drei Stellen der Versionsnummer. In der Dateibezeichnung sind nur die ersten beiden Stellen aufgeführt, dies ermöglicht es, an einzelnen Dateien Fehlerbereinigungen und dergleichen vorzunehmen, ohne dass die anderen Bestandteile von XJustiz geändert werden müssen.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Grunddaten.BNotK">
    <xs:annotation>
      <xs:documentation>Der Type.GDS.Grunddaten.BNotK enthält Meta-Informationen zum übersandten Dokument und zu dem zugehörigen Verfahren. Die darin enthaltenen Daten sind für alle Gerichtszweige und für alle Verfahrensarten von Interesse. Zusätzliche oder abgewandelte Datenstrukturen können in einem Fachmodul definiert werden.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="verfahrensdaten">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="verfahrensnummer" minOccurs="0" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Die Verfahrensnummer ist eine eindeutige Kennziffer für das gesamte betreffende Verfahren im Sinne eines Primärschlüssels. Nur übermittelt von Absendern der Justiz</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="instanzdaten" minOccurs="0" type="tns:Type.GDS.Instanzdaten.Justiz" />
            <xs:element name="beteiligung" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Beteiligung" />
            <xs:element name="herstellerinformation" minOccurs="0" type="tns:Type.GDS.Herstellerinformation" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="xjustizVersion" type="xoev-lc:String.Latin" use="required" fixed="2.2.1" />
  </xs:complexType>
  <xs:complexType name="Type.GDS.Herstellerinformation">
    <xs:annotation>
      <xs:documentation>Informationen zu der verwendeten Fachsoftware und deren Hersteller</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="nameDesProdukts" minOccurs="0" type="xoev-lc:String.Latin" />
      <xs:element name="herstellerDesProdukts" minOccurs="0" type="xoev-lc:String.Latin" />
      <xs:element name="version" minOccurs="0" type="xoev-lc:String.Latin" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Identifikation">
    <xs:sequence>
      <xs:element name="id" type="xdomea:stringUUIDType" />
      <xs:element name="nummerImUebergeordnetenContainer" minOccurs="0" type="xoev-lc:String.Latin" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Instanzdaten.Justiz">
    <xs:sequence>
      <xs:element name="instanznummer" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Die Instanznummer wird aufsteigend nummeriert, vom Absender zum Empfänger und hat nichts mit dem juristischen Begriff der 1., 2. oder 3. Instanz zu tun.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="sachgebiet" type="tns:Code.GDS.Sachgebiet" />
      <xs:element name="sachgebietszusatz" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Möglichkeit für zusätzliche Angaben bezüglich des Sachgebiets, beispielsweise in der Finanzgerichtsbarkeit die Angabe des Steuerjahres.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="instanzbehoerde" type="tns:Type.GDS.Behoerde.Justiz" />
      <xs:element name="abteilung" minOccurs="0" type="xoev-lc:String.Latin" />
      <xs:element name="verfahrensinstanznummer" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Hier wird die Verfahrensinstanznummer der Institution übergeben, sofern es eine solche gibt und diese bekannt ist. Sie dient in diesem Fall dazu, das Verfahren eindeutig zu kennzeichnen (unangängig von unterschiedlichen Schreibweisen von Aktenzeichen).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="aktenzeichen" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Bei den Instanzdaten des Gerichts wird hier des Aktenzeichen des Verfahrens übergeben, in dem die Terminladung erstellt wird. In den Instanzdaten der empfangenden Behörde wird deren Aktenzeichen übergeben.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="kurzrubrum" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>In Instanzdaten des Gerichts wird das vom Fachverfahren erzeugte Kurzrubrum übergeben. In den Instanzdaten des Empfängers wird das Element nicht übergeben.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Instanzdaten.Justiz.Erweitert">
    <xs:complexContent>
      <xs:extension base="tns:Type.GDS.Instanzdaten.Justiz">
        <xs:sequence>
          <xs:element name="verfahrensgegenstand" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Verfahrensgegenstand">
            <xs:annotation>
              <xs:documentation>Hier wird der Text der ersten Hauptforderung übergeben.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="posteingang.verfahren" minOccurs="0" type="xs:date">
            <xs:annotation>
              <xs:documentation>Hier wird das Eingangsdatum der Übergabe an das Prozessgericht übergeben. Ist der entsprechende Bereich des Prozessgerichts noch nicht für den ERV freigegeben, so ist dies das Eingangsdatum des Aktenausdrucks in Papierform. Ist der Bereich bereits freigegeben, so ist dies der Eingang der OSCI-Nachricht mit dem Aktenausdruck als Anhang im OSCI-Postfach des empfangenden Gerichts.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Link.Dokument">
    <xs:sequence>
      <xs:element name="link.dokument" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Hier kann aus Fachmodulen auf einzelne Dokumente zum Download verwiesen werden.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Mitteilung.Beteiligung.Justiz_zu_Extern">
    <xs:sequence>
      <xs:element name="nachrichtenkopf">
        <xs:complexType>
          <xs:complexContent>
            <xs:extension base="tns:Type.GDS.Nachrichtenkopf.Justiz_zu_Extern">
              <xs:sequence>
                <xs:element name="ref.instanznummer.absender" type="xoev-lc:String.Latin">
                  <xs:annotation>
                    <xs:documentation>Dieses Element enthält einen Verweis auf die Instanz. Verwiesen wird auf das Element Instanznummer. Es ist die Instanz gemeint, in der der Absender beschrieben ist.</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="ref.instanznummer.empfaenger" type="xoev-lc:String.Latin">
                  <xs:annotation>
                    <xs:documentation>Dieses Element enthält einen Verweis auf die Instanz. Verwiesen wird auf das Element Instanznummer. Es ist die Instanz gemeint, in der der Empfänger spezifiziert ist.</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="grunddaten">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="verfahrensdaten">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="instanzdaten" minOccurs="2" maxOccurs="unbounded">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="instanznummer" type="xoev-lc:String.Latin">
                          <xs:annotation>
                            <xs:documentation>Die Instanznummer wird aufsteigend nummeriert, vom Absender zum Empfänger und hat nichts mit dem juristischen Begriff der 1., 2. oder 3. Instanz zu tun.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="sachgebiet" type="tns:Code.GDS.Sachgebiet" />
                        <xs:element name="sachgebietszusatz" minOccurs="0" type="xoev-lc:String.Latin">
                          <xs:annotation>
                            <xs:documentation>Möglichkeit für zusätzliche Angaben bezüglich des Sachgebiets, beispielsweise in der Finanzgerichtsbarkeit die Angabe des Steuerjahres.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="instanzbehoerde" type="tns:Type.GDS.Behoerde" />
                        <xs:element name="verfahrensinstanznummer" minOccurs="0" type="xoev-lc:String.Latin">
                          <xs:annotation>
                            <xs:documentation>Hier wird die Verfahrensinstanznummer der Institution übergeben, sofern es eine solche gibt und diese bekannt ist. Sie dient in diesem Fall dazu, das Verfahren eindeutig zu kennzeichnen (unangängig von unterschiedlichen Schreibweisen von Aktenzeichen).</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="ereignisId" minOccurs="0" type="xoev-lc:String.Latin">
                          <xs:annotation>
                            <xs:documentation>Hier wird eine Ereignis-ID der Institution übergeben, sofern es eine solche gibt und diese bekannt ist. Sie dient in diesem Fall dazu, das Verfahren zuzuordnen.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="aktenzeichen" type="xoev-lc:String.Latin">
                          <xs:annotation>
                            <xs:documentation>Bei den Instanzdaten des Gerichts wird hier des Aktenzeichen des Verfahrens übergeben, in dem die Terminladung erstellt wird. In den Instanzdaten der empfangenden Behörde wird deren Aktenzeichen übergeben.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="beteiligung" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Beteiligung" />
                  <xs:element name="herstellerinformation" minOccurs="0" type="tns:Type.GDS.Herstellerinformation" />
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="sendungsdaten">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="papiervorgang" default="false" type="xs:boolean">
                    <xs:annotation>
                      <xs:documentation>Hier wird stets "false" übergeben, da kein Dokument verschickt wird (weder als Anhang in dieser Nachricht, noch per Papierform).</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="xjustizVersion" type="xoev-lc:String.Latin" use="required" fixed="2.2.1">
            <xs:annotation>
              <xs:documentation>Analog zum statischen Fachdatensatz erhalten die Grunddaten von Nachrichten ein Versionsattribut.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Nachrichtenkopf" abstract="true">
    <xs:annotation>
      <xs:documentation>Allgemeiner Obertyp für Nachrichten. Konkrete  Nachrichten sind in Fachmodulen als Extensions dieses Typs  umzusetzen.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="aktenzeichen.absender" minOccurs="0" type="xoev-lc:String.Latin" />
      <xs:element name="aktenzeichen.empfaenger" minOccurs="0" type="xoev-lc:String.Latin" />
      <xs:element name="erstellungszeitpunkt" type="xs:dateTime" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Nachrichtenkopf.BNotK_zu_Extern">
    <xs:annotation>
      <xs:documentation>Nachrichtenkopf für Nachrichten von der BNotK an externe Stellen (z.B. Notare)</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:Type.GDS.Nachrichtenkopf">
        <xs:sequence>
          <xs:element name="absender">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="organisation" fixed="BNotK" type="xoev-lc:String.Latin">
                  <xs:annotation>
                    <xs:documentation>Wert ist BNotK für Bundesnotarkammer</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="bereich" type="tns:Code.GDS.BNotK.Bereiche">
                  <xs:annotation>
                    <xs:documentation>Kennung für den sendenden Bereich: ZTR, ZVR, NV</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="empfaenger">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="amtstaetigkeit.id" type="xoev-lc:String.Latin">
                  <xs:annotation>
                    <xs:documentation>ID des Empfängers im Kommunikationsverzeichnis der BNotK.</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Nachrichtenkopf.BNotK_zu_Justiz">
    <xs:annotation>
      <xs:documentation>Nachrichtenkopf für Nachrichten von der BNotK an Stellen der Justiz (z.B. Verwahrgericht)</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:Type.GDS.Nachrichtenkopf">
        <xs:sequence>
          <xs:element name="absender">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="organisation" fixed="BNotK" type="xoev-lc:String.Latin">
                  <xs:annotation>
                    <xs:documentation>Wert ist BNotK für Bundesnotarkammer</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="bereich" type="tns:Code.GDS.BNotK.Bereiche">
                  <xs:annotation>
                    <xs:documentation>Kennung für den sendenden Bereich: ZTR, ZVR, NV</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:choice>
            <xs:element name="empfaenger" type="tns:Code.GDS.Gerichte" />
            <xs:element name="empfaenger.gericht.id" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Über dieses Feld kann eine Gericht Id direkt übertragen werden, z.B wenn diese in der aktuellen XJustiz Codeliste noch nicht vorhanden ist.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
          <xs:element name="ereignis" minOccurs="0" type="tns:Code.GDS.BNotK.Ereignisse">
            <xs:annotation>
              <xs:documentation>Über dieses Element können verschiedene fachliche Fälle des Nachrichtenversands automatisiert unterschieden werden. Mögliche Werte werden in der Codeliste "WL_Ereignisse_BNotK" festgelegt.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Nachrichtenkopf.Extern_zu_BNotK">
    <xs:annotation>
      <xs:documentation>Nachrichtenkopf für Nachrichten von externen Stellen (z.B. Notare) an die BNotK</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:Type.GDS.Nachrichtenkopf">
        <xs:sequence>
          <xs:element name="absender">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="amtstaetigkeit.id" type="xoev-lc:String.Latin">
                  <xs:annotation>
                    <xs:documentation>ID des Absenders im Kommunikationsverzeichnis der BNotK</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="empfaenger">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="organisation" fixed="BNotK" type="xoev-lc:String.Latin">
                  <xs:annotation>
                    <xs:documentation>Wert ist BNotK für Bundesnotarkammer</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="bereich" type="tns:Code.GDS.BNotK.Bereiche">
                  <xs:annotation>
                    <xs:documentation>Kennung für den sendenden Bereich: ZTR, ZVR, NV</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Nachrichtenkopf.Extern_zu_Justiz" abstract="true">
    <xs:annotation>
      <xs:documentation>Allgemeiner Obertyp für Nachrichten zwischen externen Kommunikationspartnern und Gerichten.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:Type.GDS.Nachrichtenkopf">
        <xs:sequence>
          <xs:element name="absender" type="xoev-lc:String.Latin" />
          <xs:element name="empfaenger" type="tns:Code.GDS.Gerichte" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Nachrichtenkopf.Justiz_zu_BNotK">
    <xs:annotation>
      <xs:documentation>Nachrichtenkopf für Nachrichten von Stellen der Justiz (z.B. Verwahrgericht) an die BNotK.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:Type.GDS.Nachrichtenkopf">
        <xs:sequence>
          <xs:choice>
            <xs:element name="absender" type="tns:Code.GDS.Gerichte" />
            <xs:element name="absender.gericht_ID" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Über dieses Feld kann eine Gericht Id direkt übertragen werden, z.B wenn diese in der aktuellen XJustiz Codeliste noch nicht vorhanden ist.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
          <xs:element name="empfaenger">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="organisation" fixed="BNotK" type="xoev-lc:String.Latin">
                  <xs:annotation>
                    <xs:documentation>Wert ist BNotK für Bundesnotarkammer</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="bereich" type="tns:Code.GDS.BNotK.Bereiche">
                  <xs:annotation>
                    <xs:documentation>Kennung für den sendenden Bereich: ZTR, ZVR, NV</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Nachrichtenkopf.Justiz_zu_Extern" abstract="true">
    <xs:annotation>
      <xs:documentation>Allgemeiner Obertyp für Nachrichten zwischen Gerichten und externen Kommunikationspartnern.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:Type.GDS.Nachrichtenkopf">
        <xs:sequence>
          <xs:element name="absender" type="tns:Code.GDS.Gerichte" />
          <xs:element name="empfaenger" type="xoev-lc:String.Latin" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Nachrichtenkopf.Justiz_zu_Justiz" abstract="true">
    <xs:annotation>
      <xs:documentation>Allgemeiner Obertyp für Nachrichten zwischen zwei  Gerichten.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:Type.GDS.Nachrichtenkopf">
        <xs:sequence>
          <xs:element name="absender" type="tns:Code.GDS.Gerichte" />
          <xs:element name="empfaenger" type="tns:Code.GDS.Gerichte" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="Type.GDS.NatuerlichePerson">
    <xs:sequence>
      <xs:element name="vollerName" maxOccurs="unbounded" type="tns:Type.GDS.Voller_Name" />
      <xs:element name="weitererName" minOccurs="0" maxOccurs="unbounded" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>z.B. Ordens-, Künstler- oder Aliasname. Wenn für einen Beteiligten nicht nur weitere Namen, sondern vollständige Alias-Identitäten (mit abweichenden Geburtsdaten, Staatsangehörigkeiten usw.) übermittelt werden sollen, muss für jede Alias-Identität ein eigenes Element Beteiligung angelegt werden, mit der Rolle "Alias-Identität" und einem Verweis auf die Rollennummer des zugehörigen Beteiligten.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="geburt" minOccurs="0" type="tns:Type.GDS.Geburt" />
      <xs:element name="tod" minOccurs="0" type="tns:Type.GDS.Tod" />
      <xs:element name="geschlecht" minOccurs="0" type="tns:Code.GDS.Geschlecht" />
      <xs:element name="familienstand" minOccurs="0" type="tns:Code.GDS.Familienstand" />
      <xs:element name="anschrift" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Anschrift">
        <xs:annotation>
          <xs:documentation>Auch Postfach-Anschriften fallen unter den Type.GDS.Anschrift.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="anschrift.gerichtsfach" minOccurs="0" type="tns:Type.GDS.Anschrift.Gerichtsfach" />
      <xs:element name="staatsangehoerigkeit" minOccurs="0" maxOccurs="unbounded" type="tns:Code.GDS.Staaten" />
      <xs:element name="personalstatut" minOccurs="0" type="tns:Code.GDS.Personalstatut" />
      <xs:element name="beruf" minOccurs="0" maxOccurs="unbounded" type="xoev-lc:String.Latin" />
      <xs:element name="telekommunikation" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Telekommunikation" />
      <xs:element name="bankverbindung" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Bankverbindung" />
      <xs:element name="auskunftssperre" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Hier kann angegeben werden, ob die Daten der betreffenden Person nicht oder nur eingeschränkt an Dritte weitergegeben werden dürfen. Werden keine Angaben gemacht, liegt keine Auskunftssperre vor!</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="grundlage" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Die Grundlage der Auskunftssperre wie z.B. wegen Gewaltschutzgesetz,  Adoptionen, Pflegefamilien, Zeugenschutz ist hier anzugeben.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="umfang" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Hier kann angegeben werden, was geschützt ist, z.B. der Name und die Anschrift (z.B. bei Zeugenschutz) oder nur die Anschrift (bei Gewaltschutz).</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="sperrstufe" minOccurs="0" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Es kann für eine Auskunftssperre eine Sperrstufe vorhanden sein. z.B. für ZStV-Schutzstufen</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="herkunftsland" minOccurs="0" maxOccurs="unbounded" type="tns:Code.GDS.Staaten" />
      <xs:element name="sprache" minOccurs="0" maxOccurs="unbounded" type="xoev-lc:String.Latin" />
      <xs:element name="auswahl_zustaendige_Institution" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Hier können für eine Person zuständige Institutionen, wie z.B. die zuständige Polizeidienststelle oder das zuständige Jugendamt erfasst werden.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice>
            <xs:element name="behoerde">
              <xs:annotation>
                <xs:documentation>Gibt es bereits allgemein bekannte Kataloge, aus denen Einträge verwendet werden sollen, so können diese Kataloge hier verwendet werden.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="katalog" type="xoev-lc:String.Latin">
                    <xs:annotation>
                      <xs:documentation>Name des verwendeten Katalogs</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="version" type="xoev-lc:String.Latin">
                    <xs:annotation>
                      <xs:documentation>Version oder Datum des verwendeten Katalogs</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="wert" type="xoev-lc:String.Latin">
                    <xs:annotation>
                      <xs:documentation>Hier wird der Wert aus dem verwendeten Katalog hinterlegt.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="sonstige" type="tns:Type.GDS.Organisation">
              <xs:annotation>
                <xs:documentation>Wird kein Katalog verwendet, so kann hier die zuständige Institution erfasst werden.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Organisation">
    <xs:sequence>
      <xs:element name="bezeichnung">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="bezeichnung.aktuell" type="xoev-lc:String.Latin" />
            <xs:element name="bezeichnung.alt" minOccurs="0" maxOccurs="unbounded" type="xoev-lc:String.Latin" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="kurzbezeichnung" minOccurs="0" type="xoev-lc:String.Latin" />
      <xs:element name="auswahl_Rechtsform" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Verwendet werden die Abkürzungen aus der Liste Code.GDS.Rechtsform, die im Schema "xj_0030_wl_rechtsform_x_x.xsd" vorliegt. Wenn die betreffende Organisationsform dort nicht aufgeführt ist, soll die vollständige Bezeichnung angegeben werden.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice>
            <xs:element name="rechtsform" type="tns:Code.GDS.Rechtsform" />
            <xs:element name="vollstaendige_Bezeichnung" type="xoev-lc:String.Latin" />
          </xs:choice>
        </xs:complexType>
      </xs:element>
      <xs:element name="sitz" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="ort" type="xoev-lc:String.Latin" />
            <xs:element name="postleitzahl" minOccurs="0" type="xoev-lc:String.Latin" />
            <xs:element name="staat" minOccurs="0" type="tns:Code.GDS.Staaten" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="registereintragung" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="gericht" type="tns:Code.GDS.Gerichte" />
            <xs:element name="registernummer" type="xoev-lc:String.Latin" />
            <xs:element name="reid" minOccurs="0" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Registered Entity Identifier</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="lei" minOccurs="0" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Legal Entity Identifier</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="euid" minOccurs="0" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>European Unique Identifier for companies and branches</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="anschrift" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Anschrift">
        <xs:annotation>
          <xs:documentation>Auch Postfach-Anschriften fallen unter den Type.GDS.Anschrift</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="anschrift.gerichtsfach" minOccurs="0" type="tns:Type.GDS.Anschrift.Gerichtsfach" />
      <xs:element name="telekommunikation" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Telekommunikation" />
      <xs:element name="bankverbindung" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Bankverbindung" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Ortsangabe">
    <xs:sequence>
      <xs:element name="ort" type="xoev-lc:String.Latin" />
      <xs:element name="staat" minOccurs="0" type="tns:Code.GDS.Staaten">
        <xs:annotation>
          <xs:documentation>Zulässige Einträge sind definiert in der Liste xj_0050_wl_staaten_x_x.xsd. Standardwert: DE. Verwendet werden die zweibuchstabigen Abkürzungen aus dem vom Auswärtigen Amt herausgegebenen Verzeichnis der Staatennamen für den amtlichen Gebrauch in der Bundesrepublik Deutschland (http://www.auswaertiges-amt.de/www/de/infoservice/download/pdf/publikationen/staatennamen.pdf). Diese entsprechen der ISO 3166-1 (http://www.iso.ch/iso/en/prods-services/iso3166ma/index.html).</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.RA.Kanzlei">
    <xs:sequence>
      <xs:element name="bezeichnung">
        <xs:annotation>
          <xs:documentation>Kanzleiname.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="bezeichnung.aktuell" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Aktueller offizieller Name der RA_Kanzlei, so wie es auch auf dem Briefkopf steht.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="bezeichnung.alt" minOccurs="0" maxOccurs="unbounded" type="xoev-lc:String.Latin">
              <xs:annotation>
                <xs:documentation>Hier soll die zuletzt geführte offizielle Bezeichnung der Kanzlei übergeben werden, wenn sich die Bezeichnung geändert hat.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="rechtsform" minOccurs="0" type="tns:Code.GDS.Rechtsform">
        <xs:annotation>
          <xs:documentation>Hier kann die Rechtsform der Kanzlei übergeben werden, z.B. GmbH, Partnerschaft.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="kanzleiform" type="tns:Code.GDS.Kanzleiform" />
      <xs:element name="anschrift" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Anschrift">
        <xs:annotation>
          <xs:documentation>Hier muss die Kanzleianschrift angebenen  werden, die verfahrensbezogen maßgeblich sein soll.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="anschrift.gerichtsfach" minOccurs="0" type="tns:Type.GDS.Anschrift.Gerichtsfach">
        <xs:annotation>
          <xs:documentation>Gerichtsfach, an das verfahrensbezogen zugestellt werden kann.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="telekommunikation" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Telekommunikation" />
      <xs:element name="ra_im_Verfahren" minOccurs="0" type="tns:Type.GDS.NatuerlichePerson">
        <xs:annotation>
          <xs:documentation>Hier kann die Daten des Rechtsanwalts / der Rechtsanwältin übergeben werden, der/die als Bevollmächtigter im Verfahren auftritt.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Rechtskraft">
    <xs:sequence>
      <xs:element name="rechtskraftdatum" type="xs:date" />
      <xs:element name="betroffener" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Ref.Rollennummer" />
      <xs:element name="gegenstand" minOccurs="0" maxOccurs="unbounded" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Für den Fall der Teilrechtskraft</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Ref.Bankverbindung">
    <xs:sequence>
      <xs:element name="ref.bankverbindung" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Hier wird die Referenz auf eine Bankverbindung im Grunddatensatz definiert. Diese Referenz kann im Fachmodul mehrfach eingesetzt werden und ist deshalb ein komplexer Typ. Ein definierter "keyref", der auf dieses Element anzuwenden ist, ist in dem jeweiligen Fachmodul angelegt.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Ref.Beteiligtennummer">
    <xs:sequence>
      <xs:element name="ref.beteiligtennummer" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Hier wird die Referenz auf eine Beteiligtennummer im Grunddatensatz definiert. Diese Referenz kann im Fachmodul mehrfach eingesetzt werden und ist deshalb ein komplexer Typ. Ein definierter "keyref", der auf dieses Element anzuwenden ist, ist in dem jeweiligen Fachmodul angelegt.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="person" maxOccurs="2" type="tns:Type.GDS.Ref.Rollennummer">
        <xs:annotation>
          <xs:documentation>Hier wird auf eine an dem Verfahren beteiligte Person über deren Beteiligtennummer im Grunddatensatz verwiesen.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Ref.Dokument">
    <xs:sequence>
      <xs:element name="ref.dokument" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Hier kann aus Fachmodulen heraus auf einzelne Dokumente referenziert werden.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Ref.Rollennummer">
    <xs:sequence>
      <xs:element name="ref.rollennummer" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Hier wird die Referenz auf eine Rollennummer im Grunddatensatz definiert. Diese Referenz kann im Fachmodul mehrfach eingesetzt werden und ist deshalb ein komplexer Typ. Ein definierter "keyref", der auf dieses Element anzuwenden ist, ist in dem jeweiligen Fachmodul angelegt.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Sendungsdaten">
    <xs:sequence>
      <xs:element name="papiervorgang" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>Flag für die Erkennung, ob es sich um einen Papiervorgang in elektronischer Form handelt. "true" = die Ladung wird auch in Papierform übergeben. "false" = die Ladung wird ausschließlich elektronisch zugestellt.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:choice minOccurs="0">
        <xs:element name="xdomea" type="xdo:T_XDOMEA" />
        <xs:element name="xdomea.2">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="akte" minOccurs="0" type="xdomea:AkteType" />
              <xs:element name="dokument" minOccurs="0" type="xdomea:DokumentType" />
              <xs:element name="vorgang" minOccurs="0" type="xdomea:VorgangType" />
            </xs:sequence>
            <xs:attribute name="xdomeaVersion" type="xoev-lc:String.Latin" use="required" fixed="2.2.0" />
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="Type.GDS.String.Max17">
    <xs:annotation>
      <xs:documentation>Begrenzung des strings auf 17 Stellen.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xoev-lc:String.Latin">
      <xs:maxLength value="17" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="Type.GDS.Teilakte">
    <xs:sequence>
      <xs:element name="identifikation" type="tns:Type.GDS.Identifikation" />
      <xs:element name="teilaktentyp" type="tns:Code.GDS.Teilaktentyp" />
      <xs:element name="anzeigename" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Freitextfeld für ergänzende Angaben zum Teilaktentyp</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="akteneinsicht" default="true" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>StV muss auf "Nein" schalten</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="letzte_Paginierung_pro_Teilakte" minOccurs="0" type="xoev-lc:String.Latin" />
      <xs:element name="inhalt" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="teilakte" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Teilakte" />
            <xs:element name="dokument" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Dokument" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="anwendungsspezifische_Erweiterung" minOccurs="0" maxOccurs="unbounded" type="xdomea:AnwendungsspezifischeErweiterungType" />
      <xs:element name="ruecksendung_EEB_erforderlich" default="false" type="xs:boolean" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Telekommunikation">
    <xs:sequence>
      <xs:element name="telekommunikationsart" type="tns:Code.GDS.Telekommunikationsart" />
      <xs:element name="telekommunikationszusatz" minOccurs="0" type="tns:Code.GDS.Telekommunikationszusatz" />
      <xs:element name="verbindung" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Hier wird die Telefonnummer, Faxnummer, E-Mail-Adresse oder dergleichen mitgeteilt.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Terminsdaten">
    <xs:sequence>
      <xs:element name="terminsId" minOccurs="0" type="xoev-lc:String.Latin" />
      <xs:element name="spruchkoerper" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Hier kann der Spruchkörper angegeben werden, vor dem der Termin stattfindet (z.B. Große Strafkammer, Senat, aber auch Einzelrichter, beauftragter Richter, Vorsitzender usw.)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="oeffentlich" type="xs:boolean" />
      <xs:element name="Auswahl_Terminsort">
        <xs:complexType>
          <xs:choice>
            <xs:element name="gerichtsort">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="gebaeude" minOccurs="0" type="xoev-lc:String.Latin" />
                  <xs:element name="anschrift" minOccurs="0" type="tns:Type.GDS.Anschrift" />
                  <xs:element name="stockwerk" minOccurs="0" type="xoev-lc:String.Latin" />
                  <xs:element name="raum" type="xoev-lc:String.Latin" />
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="lokaltermin">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="beschreibung" minOccurs="0" type="xoev-lc:String.Latin">
                    <xs:annotation>
                      <xs:documentation>Hier kann eine kurze Beschreibung des Lokaltermins angegeben werden.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="anschrift" minOccurs="0" type="tns:Type.GDS.Anschrift" />
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:choice>
        </xs:complexType>
      </xs:element>
      <xs:element name="terminszeit">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="terminsdatum" type="xs:date" />
            <xs:choice>
              <xs:element name="terminsuhrzeit" type="xs:time">
                <xs:annotation>
                  <xs:documentation>Uhrzeit gemäss DIN ISO 8601 (hh:mm)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="terminszeitangabe" type="xoev-lc:String.Latin">
                <xs:annotation>
                  <xs:documentation>Hier ist Raum für eine textuelle Zeitangabe, z.B. "vormittags", "zwischen 14 und 16 Uhr" etc.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:choice>
            <xs:element name="terminsdauer" minOccurs="0" type="xs:duration">
              <xs:annotation>
                <xs:documentation>Zeitdauer wie ISO-Zeitformat</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="terminsart" type="tns:Code.GDS.Terminsart" />
      <xs:element name="teilnehmer" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Dieses Element enthält einen Verweis auf einen Beteiligten. Verwiesen wird auf den Wert der Rollennummer in "Beteiligung/Rolle". Für jeden Beteiligten, auf den verwiesen wird, muss ein entsprechendes Element "Beteiligung" vorhanden sein.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:complexContent>
            <xs:extension base="tns:Type.GDS.Ref.Rollennummer">
              <xs:sequence>
                <xs:element name="ladungszeit" minOccurs="0">
                  <xs:annotation>
                    <xs:documentation>Dieses Element ermöglicht eine "gestaffelte" Ladung. Das heißt: Für jeden Beteiligten kann gesondert bestimmt werden, wann er erscheinen soll.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element name="ladungsdatum" minOccurs="0" type="xs:date" />
                      <xs:choice>
                        <xs:element name="ladungsuhrzeit" type="xs:time">
                          <xs:annotation>
                            <xs:documentation>Uhrzeit gemäss DIN ISO 8601 (hh:mm)</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="ladungszeitangabe" type="xoev-lc:String.Latin">
                          <xs:annotation>
                            <xs:documentation>Hier ist Raum für eine textuelle Zeitangabe,  z.B. "vormittags", "zwischen 14 und 16 Uhr" etc.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                      </xs:choice>
                      <xs:element name="ladungsdauer" minOccurs="0" type="xs:duration">
                        <xs:annotation>
                          <xs:documentation>Zeitdaumer wie ISO-Zeitformat</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
                <xs:element name="ladungszusatz" minOccurs="0" type="xoev-lc:String.Latin" />
              </xs:sequence>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Tod">
    <xs:sequence>
      <xs:element name="sterbedatum" minOccurs="0" type="tns:Type.GDS.Datumsangabe">
        <xs:annotation>
          <xs:documentation>Datumsfeld nach ISO 8601 (yyyy-mm-dd)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="sterbeort" minOccurs="0" type="tns:Type.GDS.Ortsangabe" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Verfahrensausgangsmitteilung.Justiz_zu_Extern">
    <xs:sequence>
      <xs:element name="nachrichtenkopf">
        <xs:complexType>
          <xs:complexContent>
            <xs:extension base="tns:Type.GDS.Nachrichtenkopf.Justiz_zu_Extern">
              <xs:sequence>
                <xs:element name="ref.instanznummer.absender" type="xoev-lc:String.Latin">
                  <xs:annotation>
                    <xs:documentation>Dieses Element enthält einen Verweis auf die Instanz. Verwiesen wird auf das Element Instanznummer. Es ist die Instanz gemeint, in der der Absender beschrieben ist.</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="ref.instanznummer.empfaenger" type="xoev-lc:String.Latin">
                  <xs:annotation>
                    <xs:documentation>Dieses Element enthält einen Verweis auf die Instanz. Verwiesen wird auf das Element Instanznummer. Es ist die Instanz gemeint, in der der Empfänger spezifiziert ist.</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="grunddaten">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="verfahrensdaten">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="instanzdaten" minOccurs="2" maxOccurs="unbounded">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="instanznummer" type="xoev-lc:String.Latin">
                          <xs:annotation>
                            <xs:documentation>Die Instanznummer wird aufsteigend nummeriert, vom Absender zum Empfänger und hat nichts mit dem juristischen Begriff der 1., 2. oder 3. Instanz zu tun.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="sachgebiet" type="tns:Code.GDS.Sachgebiet" />
                        <xs:element name="sachgebietszusatz" minOccurs="0" type="xoev-lc:String.Latin">
                          <xs:annotation>
                            <xs:documentation>Möglichkeit für zusätzliche Angaben bezüglich des Sachgebiets, beispielsweise in der Finanzgerichtsbarkeit die Angabe des Steuerjahres.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="instanzbehoerde" type="tns:Type.GDS.Behoerde" />
                        <xs:element name="verfahrensinstanznummer" minOccurs="0" type="xoev-lc:String.Latin">
                          <xs:annotation>
                            <xs:documentation>Hier wird die Verfahrensinstanznummer der Institution übergeben, sofern es eine solche gibt und diese bekannt ist. Sie dient in diesem Fall dazu, das Verfahren eindeutig zu kennzeichnen (unangängig von unterschiedlichen Schreibweisen von Aktenzeichen).</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="ereignisId" minOccurs="0" type="xoev-lc:String.Latin">
                          <xs:annotation>
                            <xs:documentation>Hier wird eine Ereignis-ID der Institution übergeben, sofern es eine solche gibt und diese bekannt ist. Sie dient in diesem Fall dazu, das Verfahren zuzuordnen.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="aktenzeichen" type="xoev-lc:String.Latin">
                          <xs:annotation>
                            <xs:documentation>Bei den Instanzdaten des Gerichts wird hier des Aktenzeichen des Verfahrens übergeben, in dem die Terminladung erstellt wird. In den Instanzdaten der empfangenden Behörde wird deren Aktenzeichen übergeben.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="beteiligung" minOccurs="0" maxOccurs="unbounded">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="rolle" maxOccurs="unbounded">
                          <xs:annotation>
                            <xs:documentation>Jeder Beteiligte hat eine oder mehrere Rollen, z.B. Kläger, Beklagter, Zeuge, Dolmetscher, Rechtsanwalt.</xs:documentation>
                          </xs:annotation>
                          <xs:complexType>
                            <xs:sequence>
                              <xs:element name="loeschen" minOccurs="0" default="false" type="xs:boolean">
                                <xs:annotation>
                                  <xs:documentation>Diese Information bezieht sich auf die Löschung einer Rolle aus dem Verfahren. Wenn dieses Element mit true belegt ist, gibt der Absender der XML Info zu erkennen, dass er die Löschung der Rolle vorschlägt. Es liegt in der Entscheidung des Empfängers, ob diese Daten gelöscht werden oder nicht.</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                              <xs:element name="rollennummer" type="xoev-lc:String.Latin">
                                <xs:annotation>
                                  <xs:documentation>Die Rollennummer wird als eindeutige Kennziffer innerhalb des XML-Dokuments benötigt, um aus anderen Elementen innerhalb des XML-Dokuments heraus auf Beteiligte und Rollen verweisen zu können.  Die Rollennummer kann aus einem Datenbank-Schlüssel abgeleitet werden, der dem Beteiligten in der Datenbank eines Gerichts, einer Staatsanwaltschaft oder einer sonstigen Behörde zur eindeutigen Identifizierung zugeordnet ist. Zwingend erforderlich ist dies nicht. Sofern die einzelnen Kommunikationsteilnehmer intern unterschiedliche Datenbank-Schlüssel verwenden, können diese im Element Rollen_ID (siehe unten) abgelegt werden. Die Rollennummer kann dann frei bestimmt werden, solange sie nur innerhalb eines XML-Dokuments eindeutig bleibt.</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                              <xs:element name="rollenId" minOccurs="0" maxOccurs="unbounded">
                                <xs:complexType>
                                  <xs:sequence>
                                    <xs:element name="id" type="xoev-lc:String.Latin">
                                      <xs:annotation>
                                        <xs:documentation>Das String-Feld "ID" ist für die Aufnahme des Primärschlüssels der Rolle der referenzierten Instanz gedacht. Auf diese Weise ist es möglich, bei Kommunikation zwischen verschiedenen Instanzen (wobei auch außergerichtliche Teile des Verfahrens als Instanzen zu verstehen sind (z. Bsp. Vorgänge bei Verwaltungsbehörden oder Polizei)) die Primärschlüssel verschiedenster Anwendungen in verschiedenen Behörden mittels X-Justiz für eine erleichterte automatische Verarbeitung auszutauschen. Die Referenzierung der Instanzdatenist möglich, für verschiedene Instanzen können verschiedene Rollenbezeichnungen für einen Beteiligten mitgeteilt werden.</xs:documentation>
                                      </xs:annotation>
                                    </xs:element>
                                    <xs:element name="ref.instanznummer" type="xoev-lc:String.Latin">
                                      <xs:annotation>
                                        <xs:documentation>Dieses Element enthält einen Verweis auf die Instanz, bei der die oben angegebene ID verwendet wird. Verwiesen wird auf das Element Instanznummer.</xs:documentation>
                                      </xs:annotation>
                                    </xs:element>
                                  </xs:sequence>
                                </xs:complexType>
                              </xs:element>
                              <xs:element name="rollenbezeichnung" type="tns:Code.GDS.Rollenbezeichnung" />
                              <xs:element name="nr" default="1" type="xs:integer">
                                <xs:annotation>
                                  <xs:documentation>Wenn es in einem Verfahren mehrere gleichartige Beteiligte gibt (z. B. mehrere Kläger), sind diese fortlaufend zu nummerieren. Jede Kombination aus Rollenbezeichnung und Nummer (z.B. Kläger 1, Kläger 2, Beklagter 1 usw.) darf pro Verfahren nur einmal vorkommen.  Mit XML-Mitteln kann nur die Eindeutigkeit innerhalb des jeweiligen XJustiz-Datensatzes überprüft werden.  Für die überprüfung der verfahrensweiten Eindeutigkeit sind die jeweiligen Kommunikationsteilnehmer verantwortlich.</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                              <xs:element name="geschaeftszeichen" minOccurs="0" type="xoev-lc:String.Latin">
                                <xs:annotation>
                                  <xs:documentation>Hier ist Raum für ein eigenes Geschäftszeichen des Beteiligten, z.B. die Schadensnummer einer Versicherung oder die Mandatsnummer eines Rechtsanwalts.</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                              <xs:element name="referenz" minOccurs="0" maxOccurs="unbounded" type="tns:Type.GDS.Ref.Rollennummer">
                                <xs:annotation>
                                  <xs:documentation>Hier kann ein Bezug zu einer anderem Rolle angegeben werden. Einige Rollen haben einen Bezug zu einer anderen Beteiligung. Typisches Beispiel sind Rechtsanwälte.Diese handeln in aller Regel als Prozessbevollmächtigte für einen anderen Beteiligten.Im vorliegenden Feld wird dann vermerkt, für welchen Beteiligten (ggf. auch in welcher Rolle ) der Rechtsanwalt handelt.  Für jede Rolle, auf die verwiesen wird, muss ein entsprechendes Element "Beteiligung/Rolle" vorhanden sein. Verwiesen wird auf die Rollennummer.</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                              <xs:element name="anrede" minOccurs="0" type="xoev-lc:String.Latin">
                                <xs:annotation>
                                  <xs:documentation>Hier können Anredeformen übermittelt werden, die in einem Fachverfahren für den Beteiligten in der betreffenden Rolle gespeichert sind, z.B. "Herrn Rechtsanwalt" oder "Frau Notarin".</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                              <xs:element name="dienstbezeichnung" minOccurs="0" type="xoev-lc:String.Latin">
                                <xs:annotation>
                                  <xs:documentation>Das Element Dienstbezeichnung wird z.B. gebraucht, um Polizisten als Zeugen korrekt laden zu können.</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                              <xs:element name="funktionsbezeichnung" minOccurs="0" type="xoev-lc:String.Latin">
                                <xs:annotation>
                                  <xs:documentation>Hier können Funktionsbezeichnungen übergeben werden, die eine Person näher beschreiben, wobei nicht ihre Rolle im gerichtlichen Verfahren gemeint ist.</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                            </xs:sequence>
                          </xs:complexType>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="herstellerinformation" minOccurs="0" type="tns:Type.GDS.Herstellerinformation" />
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="sendungsdaten">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="papiervorgang" default="false" type="xs:boolean">
                    <xs:annotation>
                      <xs:documentation>Hier wird stets "false" übergeben, da kein Dokument verschickt wird (weder als Anhang in dieser Nachricht, noch per Papierform).</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="xjustizVersion" type="xoev-lc:String.Latin" use="required" fixed="2.2.1">
            <xs:annotation>
              <xs:documentation>Analog zum statischen Fachdatensatz erhalten die Grunddaten von Nachrichten ein Versionsattribut.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Verfahrensgegenstand">
    <xs:sequence>
      <xs:element name="gegenstand" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Hier steht der Verfahrensgegenstand, z.B. "Schadensersatz" oder "Diebstahl".  Für die zulässigen Werte kann in den Fachmodulen jeweils eine fachspezifische Codeliste definiert werden.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="gegenstandswert" minOccurs="0" type="tns:Type.GDS.Geldbetrag">
        <xs:annotation>
          <xs:documentation>Hier wird - sofern vorhanden - der Streitwert oder Gegenstandswert des Verfahrens angegeben.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Type.GDS.Voller_Name">
    <xs:sequence>
      <xs:element name="vorname" minOccurs="0" type="xoev-lc:String.Latin" />
      <xs:element name="rufname" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Der Rufname gibt bei mehreren Vornamen an, welcher Vorname der Rufname ist. Der Rufname wird häufig als eindeutiges Element benötigt.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="titel" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Hier werden akademische und nichtakademische Titel aufgeführt, die vor dem Vornamen stehen, z.B. "Professor", "Dr.", "Prof. Dr." usw. Werden mehrere Titel benötigt, ist eine Reihenfolge zu berücksichtigt: 1) Militärischer Rang 2.) Akademischer Titel 3.) Nichtakademischer Grad</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="namensvorsatz" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Hier werden Adelstitel aufgeführt, die vor dem Nachnamen stehen, z.B. "Graf von"</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="nachname" minOccurs="0" type="xoev-lc:String.Latin" />
      <xs:element name="geburtsname" minOccurs="0" type="xoev-lc:String.Latin" />
      <xs:element name="nachname.alt" minOccurs="0" maxOccurs="unbounded" type="xoev-lc:String.Latin" />
      <xs:element name="namenszusatz" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Hier ist Raum für Zusätze, die hinter den Nachnamen stehen, z.B. "MdB" oder "LLM."</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="Type.GDS.Zeitangabe">
    <xs:annotation>
      <xs:documentation>Variabler Typ für die Angabe der Uhrzeit in der Form hh:mm:ss. Minuten- und Sekundenangaben sind optional. Die Zeitzone wird nicht angegeben.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xoev-lc:String.Latin">
      <xs:pattern value="\d{1,2}(:\d{2}){0,2}" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="Type.GDS.Zinsen">
    <xs:sequence>
      <xs:element name="zinsberechnung" type="tns:Code.GDS.Zinsberechnung">
        <xs:annotation>
          <xs:documentation>Mögliche Werte: "laufende Zinsen"; "vom Gericht ausgerechneter Zinsbetrag" (bis zum Erlass des Mahnbescheids oder bis zum Zinsende-Datum, das der Antragsteller angegeben hat und das vor MB-Antragsdatum liegt); "vom Antragsteller ausgerechnete Zinsen"..</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ref.anspruchsnummer" maxOccurs="unbounded" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>Bezug auf die Anspruchsnummer</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:choice>
        <xs:element name="zinssatz" type="xoev-lc:String.Latin">
          <xs:annotation>
            <xs:documentation>Die Höhe des Zinssatz in %</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="basiszinssatz" type="xoev-lc:String.Latin">
          <xs:annotation>
            <xs:documentation>ODER: % über Basiszinssatz</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
      <xs:element name="zinsmerkmal" type="tns:Code.GDS.Zinsmerkmal">
        <xs:annotation>
          <xs:documentation>Mögliche Werte sind hier: jährlich, monatlich,täglich.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="berechnungsgrundlage" minOccurs="0" type="xoev-lc:String.Latin">
        <xs:annotation>
          <xs:documentation>zu_verzinsender_Betrag; Hier wird ein Betrag für die Zinsberechnung angegeben, wenn dieser abweichend von der Hauptsacheforderung ist.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="zinsbeginn" minOccurs="0" type="xs:date">
        <xs:annotation>
          <xs:documentation>Zinsbeginn, wenn nicht ab Zustellung des Mahnbescheids angegeben ist; Berechnung für die Zeit vom</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="zinsende" minOccurs="0" type="xs:date">
        <xs:annotation>
          <xs:documentation>Berechnung für die Zeit der Zinsen bis zu einem Enddatum.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="zinsbetrag" type="tns:Type.GDS.Geldbetrag">
        <xs:annotation>
          <xs:documentation>Ausgerechneter Zinsbetrag</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:schema>

