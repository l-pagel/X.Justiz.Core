                      </xs:annotation>
                    </xs:element>
                    <xs:element name="staat_Gericht" type="tns:Code.GDS.Staaten" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="bezahlung_Gerichtsgebuehren">
