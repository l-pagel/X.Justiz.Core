            <xs:sequence>
              <xs:element name="anerkennung_Forderung">
                <xs:complexType>
                  <xs:choice>
                    <xs:element name="flag_Ja" fixed="true" type="xs:boolean" />
                    <xs:element name="auswahl_Nein_Teilweise">
                      <xs:annotation>
                        <xs:documentation>Wenn mit "Nein" oder "Teilweise" geantwortet wurde, sollten die Gründe dafür angegeben werden.</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="flag_Nein" default="false" type="xs:boolean" />
                          <xs:element name="flag_Teilweise" default="false" type="xs:boolean" />
                          <xs:element name="begruendung_kein_SmallClaims" minOccurs="0" type="xoev-lc:String.Latin">
                            <xs:annotation>
                              <xs:documentation>Begründung, warum die Klage nicht in den Anwendungsbereich des europäischen Verfahrens für geringfügige Forderungen fällt.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="sonstige_Gruende" minOccurs="0" type="xoev-lc:String.Latin" />
