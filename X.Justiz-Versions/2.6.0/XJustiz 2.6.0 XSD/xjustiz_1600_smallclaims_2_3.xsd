                    <xs:element name="sprach_Code" type="xoev-lc:String.Latin" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="zahlung_Urteil" type="tns:Type.SC.Zahlung" />
              <xs:element name="statement">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="ort_Datum_Unterschrift">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="name" type="xoev-lc:String.Latin">
                            <xs:annotation>
