                    <xs:element name="zeugenbeweis_Bestreitung" minOccurs="0" type="xoev-lc:String.Latin" />
                    <xs:element name="sonstige_Beweismittel_Bestreitung" minOccurs="0" type="xoev-lc:String.Latin" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="muendliche_Verhandlung">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="flag_Muendliche_Verhandlung" default="false" type="xs:boolean" />
                    <xs:element name="begruendung_Muendliche_Verhandlung" minOccurs="0" type="xoev-lc:String.Latin" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="erstattung_Verfahrenskosten">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="flag_Erstattung_Verfahrenskosten" default="false" type="xs:boolean" />
                    <xs:element name="begruendung_Erstattung_Verfahrenskosten" minOccurs="0" type="xoev-lc:String.Latin" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="widerklage">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="flag_Widerklage" default="false" type="xs:boolean" />
                    <xs:element name="begruendung_Widerklage" minOccurs="0" type="xoev-lc:String.Latin" />
                  </xs:sequence>
