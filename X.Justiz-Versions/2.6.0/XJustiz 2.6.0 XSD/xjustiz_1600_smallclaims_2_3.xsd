                      </xs:complexType>
                    </xs:element>
                    <xs:element name="beweismittel" maxOccurs="3">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="urkundenbeweis" minOccurs="0" type="xoev-lc:String.Latin" />
                          <xs:element name="zeugenbeweis" minOccurs="0" type="xoev-lc:String.Latin" />
                          <xs:element name="sonstige_Beweismittel" minOccurs="0" type="xoev-lc:String.Latin" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="klage_Begruendung" minOccurs="0" type="xoev-lc:String.Latin" />
                    <xs:element name="erstattung_Verfahrenskosten">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="flag_Erstattung" default="false" type="xs:boolean" />
                          <xs:element name="naehere_Angaben_Erstattung" minOccurs="0" type="xoev-lc:String.Latin" />
                          <xs:element name="zinsforderung_Erstattung" type="tns:Type.SC.Zinsforderung" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="gerichtliche_Zustaendigkeit">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="begruendung" maxOccurs="8" type="tns:Code.SC.Gerichtliche_Zustaendigkeit" />
                    <xs:element name="sonstiger_Zustaendigkeitsgrund" minOccurs="0" type="xoev-lc:String.Latin" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="grenzueberschreitender_Sachverhalt">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="staat_Aufenthalt_Klaeger" type="tns:Code.GDS.Staaten">
                      <xs:annotation>
                        <xs:documentation>Staat des Wohnsitzes oder gewöhlichen Aufenthalts des Klägers</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="staat_Aufenthalt_Beklagter" type="tns:Code.GDS.Staaten">
