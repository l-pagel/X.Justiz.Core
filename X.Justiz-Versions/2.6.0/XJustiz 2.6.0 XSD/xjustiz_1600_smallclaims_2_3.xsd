        <xs:element name="grunddaten" type="tns:Type.SC.Grunddaten" />
        <xs:element name="fachdaten">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="forderung" maxOccurs="2">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="verfahrensgegenstand">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="gegenstand" minOccurs="0" type="xoev-lc:String.Latin">
                            <xs:annotation>
                              <xs:documentation>Gegenstand der Forderung</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="gegenstandswert">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="zahl" type="xs:double" />
                                <xs:element name="auswahl_Waehrung">
                                  <xs:complexType>
                                    <xs:choice>
                                      <xs:element name="waehrung_Art" type="tns:Code.GDS.Waehrung" />
                                      <xs:element name="sonstige_Waehrung" type="xoev-lc:String.Latin" />
