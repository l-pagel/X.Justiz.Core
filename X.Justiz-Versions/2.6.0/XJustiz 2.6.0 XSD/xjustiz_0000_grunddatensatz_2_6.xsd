      <xs:documentation>Nachrichtenkopf für Nachrichten von Stellen der Justiz (z.B. Verwahrgericht) an die BNotK.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:Type.GDS.Nachrichtenkopf">
        <xs:sequence>
          <xs:choice>
            <xs:element name="absender" type="tns:Code.GDS.Gerichte" />
