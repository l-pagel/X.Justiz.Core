  <xs:complexType name="Type.GDS.Nachrichtenkopf.Justiz_zu_Extern" abstract="true">
    <xs:annotation>
      <xs:documentation>Allgemeiner Obertyp für Nachrichten zwischen Gerichten und externen Kommunikationspartnern.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:Type.GDS.Nachrichtenkopf">
        <xs:sequence>
          <xs:element name="absender" type="tns:Code.GDS.Gerichte" />
          <xs:element name="empfaenger" type="xoev-lc:String.Latin" />
