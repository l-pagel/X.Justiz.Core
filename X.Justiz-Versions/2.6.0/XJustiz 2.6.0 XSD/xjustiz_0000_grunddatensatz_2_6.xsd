              <xs:annotation>
                <xs:documentation>Über dieses Feld kann eine Gericht Id direkt übertragen werden, z.B wenn diese in der aktuellen XJustiz Codeliste noch nicht vorhanden ist.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
          <xs:element name="empfaenger">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="organisation" fixed="BNotK" type="xoev-lc:String.Latin">
                  <xs:annotation>
                    <xs:documentation>Wert ist BNotK für Bundesnotarkammer</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="bereich" type="tns:Code.GDS.BNotK.Bereiche">
                  <xs:annotation>
                    <xs:documentation>Kennung für den sendenden Bereich: ZTR, ZVR, NV</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
