  <xs:complexType name="Type.GDS.Nachrichtenkopf.Extern_zu_Justiz" abstract="true">
    <xs:annotation>
      <xs:documentation>Allgemeiner Obertyp für Nachrichten zwischen externen Kommunikationspartnern und Gerichten.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:Type.GDS.Nachrichtenkopf">
        <xs:sequence>
          <xs:element name="absender" type="xoev-lc:String.Latin" />
          <xs:element name="empfaenger" type="tns:Code.GDS.Gerichte" />
